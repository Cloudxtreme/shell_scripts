<script type="text/javascript">function handle_error(section,data){var message='';if(section=='bitly'){message="Having some trouble making a link right now, try again later.";}else if(section=="domain"){message="Oops! Please enter a valid domain (e.g. mydomain.com).";}else{message="Something has gone horribly wrong. Try again later.";}
