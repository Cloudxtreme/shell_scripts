function bitly_compress(validated_domain){$.ajax({url:'bitly.php',dataType:'html',data:{domain:validated_domain,special_offer:"20referral",redirect_url:"/webhosting"},success:function(data){if(data){build_links(data);$("#link_builder_results").slideDown(200);$('#social').slideDown(200);$('.heading-section').animate({"padding-bottom":30},200);}else{handle_error("bitly");}},error:function(data){handle_error("bitly",data);},});}
function build_social(url){text=encodeURIComponent("My web host, Media Temple, is giving my friends $25 in free hosting. Use this link to get started.");img=encodeURIComponent('http://s1.mt-cdn.net/referrals/img/mt-referral-program.jpg');url=encodeURIComponent(url);$("#social a.facebook").attr('href','http://www.facebook.com/sharer.php?u='+url);$("#social a.twitter").attr('href','http://twitter.com/share?text='+text+" "+url);$("#social a.pinterest").attr('href','http://pinterest.com/pin/create/button/?url='+url+'&media='+img+'&description='+text);$("#social a.tumblr").attr('href','http://www.tumblr.com/share/photo?source='+img+'&caption='+text+'&clickthru='+url);}
function build_links(domain){$('#result_link').val(domain);$('#social img.qr').attr('src',domain+'.qrcode').closest('a').attr('href',domain+'.qrcode');build_social(domain);}
function validate_domain(domain){var domain_regex=/^[a-z0-9]+(?:[\-\.]{1}[a-z0-9]+)*\.[a-z]{2,5}$/;if(false==domain_regex.test(domain.toLowerCase())||''==domain){handle_error("domain",domain);return false;}
return true;}
