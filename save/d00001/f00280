<script>
	$('ul#tabs li a').click(function () {
		var href = $(this).attr('href');
		href = href.substring(href.indexOf('#'));
		var li = $(this).parent();
		var ul = li.parent();
		ul.find('li.active').removeClass('active');
		li.addClass('active');
		var current = $('div#tab_load div.tab_content');
		var newTab = $(href);
		current.hide();
		newTab.show();
		return false;
	});
</script>

