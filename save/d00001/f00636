<script src="/js_css/main_promo.js" type="text/javascript"></script>
<script language="javascript">
// main page only
var SCEMSPromo = '';


var maxLGPromo = (randPromoLG.length);
var maxMEDPromo = (randPromoMED.length);
var defaultLGPromo ='';
defaultLGPromo = 

'<table width="190" cellspacing="0" cellpadding="0" border="0"><tr><td width="190" align="left" valign="top"><a href="/students/idealabs/walking_on_the_moon.html"><img src="/images/global/promos/large/pr_lrg_apollo.jpg" border="0" alt="Apollo 11: Walking on the Moon" /></a></td></tr></table>'
;

var defaultMEDPromo = '';
defaultMEDPromo = 
'';
function writeLGPromo() {
	SCEMSPromo = '';
	var randLGAccess = Math.floor(((Math.random()*1000000)%maxLGPromo));
	//alert(maxLGPromo + ' - ' + randLGAccess);
	SCEMSPromo = randPromoLG[randLGAccess];
}

function writeMEDPromo() {
	SCEMSPromo = '';
	var randMEDAccess = Math.floor(((Math.random()*1000000)%maxMEDPromo));
	//alert(maxMEDPromo + ' - ' + randMEDAccess);
	SCEMSPromo += randPromoMED[randMEDAccess];
	SCEMSPromo += '<table width="1" border="0" cellspacing="0" cellpadding="0"><tr><td><img src="/images/global/spacer.gif" width=1 height=1 border=0 alt=""></td></tr></table>';
	if ( (parseInt(randMEDAccess) + 1) == maxMEDPromo ) {
		randMEDAccess = -1;
	}
	SCEMSPromo += randPromoMED[parseInt(randMEDAccess) +1];
}


if ( (maxLGPromo > 0) && (maxMEDPromo > 0) ) {
	var whichAccess = Math.floor(((Math.random()*1000000)%2));
	if (whichAccess) {
		//alert('show Large Promo');
		writeLGPromo();
	}
	else {
		//alert('show Medium Promos');
		writeMEDPromo();
	}
}
else if ((maxLGPromo > 0) && (maxMEDPromo == 0)) {writeLGPromo();}

else if ((maxLGPromo == 0) && (maxMEDPromo > 0)) {writeMEDPromo();}

else if ((maxLGPromo == 0) && (maxMEDPromo == 0)) {
	SCEMSPromo += defaultLGPromo;
	SCEMSPromo += defaultMEDPromo; 
}

</script>
