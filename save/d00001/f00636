function homeImgRoll(img, state) {
//button roll
		(state) ? document.images[img].src = eval(img + '_home_button_over.src') : document.images[img].src = eval(img + '_home_button_off.src');
//text roll
		(state) ? document.images['home_text'].src = eval(img + '_home_text_on.src') : document.images['home_text'].src = eval('home_text_off.src');

}
