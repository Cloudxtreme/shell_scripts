<script type="text/javascript">
$(document).ready(function(){
    
        $('form#signupfree').hide()
    
    $('a#freelink').click(function(e){
        e.preventDefault()
        $('form#signupfree').show()
        $('form#signuppaid').hide()
    })
    $('a#paidlink').click(function(e){
        e.preventDefault()
        $('form#signuppaid').show()
        $('form#signupfree').hide()
    })
})
</script>

