hoje = new Date()
dia = hoje.getDate()
dias = hoje.getDay()
mes = hoje.getMonth()
ano = hoje.getYear()

if (dia < 10)
dia = "0" + dia

if (ano < 2000)
ano = 1900 + ano

function NArray (n) 
{
this.length = n
}

NomeDiaWMOnline = new NArray(7)

NomeDiaWMOnline[0] = "Domingo"
NomeDiaWMOnline[1] = "Segunda-feira"
NomeDiaWMOnline[2] = "Ter&ccedil;a-feira"
