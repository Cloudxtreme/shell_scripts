jQuery(document).ready( function($) {
    var activation_function = function(e) {
        if ($(e.target).is('#whereami, #navigation_selector')) {
            $('#navigation').toggleClass('active');
            e.preventDefault();
        }
    };
    $('#navigation_selector').click( function (e) {
        e.preventDefault();
    });
    if ("ontouchstart" in document) {
        $('#navigation').removeClass('no-touch');
        $('#whereami').on('touchstart', activation_function);
    } else {
        $('#whereami').click(activation_function);
    }
    $('body').click(function(e){ 
        if ( ! $(e.target).is('#whereami, #navigation_selector')) {
            $('#navigation').removeClass('active'); 
        }
    });
});
