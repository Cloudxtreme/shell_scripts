		<script type="text/javascript" language="javascript">
function showOnly( status, country, continent ) {
	var fadeTime = 1.0;
	var appearTime = 1.0;
	var doc = document.getElementsByTagName('dl');
	var myReg1 = new RegExp( "country_" );
	var myReg2 = new RegExp( "country_" + country + "_" + continent + "_" );
  for (var i = 0; i < doc.length; i++){
    if (doc[i].id.match( myReg1 ) ) {
      if ( doc[i].id.match( myReg2 ) ) {
        if (status == 1) {
          $("#"+doc[i].id).fadeIn(appearTime);
        }
      } else {
        if (status == 0) {
          $("#"+doc[i].id).fadeOut(fadeTime);
        }
      }
    }
  }
  var doc = document.getElementsByTagName('h3');
  var myReg1 = new RegExp( "continent_" );
  var myReg2 = new RegExp( "continent_" + continent );
  for (var i = 0; i < doc.length; i++){
    if (doc[i].id.match( myReg1 ) ) {
      if ( doc[i].id.match( myReg2 ) ) {
        if (status == 1) {
          $("#"+doc[i].id).fadeIn(appearTime);
        }
      } else {
        if (status == 0) {
          $("#"+doc[i].id).fadeOut(appearTime);
        }
      }
    }
  }
if(status == 0){
setTimeout( "showOnly( 1, '" + country + "', '" + continent + "')", (fadeTime * 1000) + 100 );
}
}

function showOnly2(status, continent, country) {
	var appearTime = 1.0;
	$('dl[id*="country_"]').fadeOut(appearTime);
	$('h3[id*="continent_"]').fadeOut(appearTime);
	if (country!='') {
		$('dl[id^="country_'+continent+'_'+country+'"]').fadeIn(appearTime);
		$('h3[id^="continent_'+continent+'"]').fadeIn(appearTime);
	} else if (continent!='') {
		$('h3[id^="continent_'+continent+'"]').fadeIn(appearTime);
		$('dl[id^="country_3_"]').fadeIn(appearTime);
	} else {
		$('dl[id*="country_"]').fadeIn(appearTime);
		$('h3[id*="continent_"]').fadeIn(appearTime);
	}
}
	
function showAll2() {
	showOnly2(0, '', '');
}

function readyacademy(){
if ($("a.galleryimg").length != 0){
$("a.galleryimg").fancybox({"overlayShow":false,"hideOnContentClick":true});
}
}

$(document).ready(function(){readyacademy();});
		
function showAll() {
  showOnly( 0, '[a-zA-Z0-9]*', '[0-9]*' );
}
</script><script type="text/javascript">
function openBigMap(){
$("#minimap").hide();
$("#largemap").show();

$("#largemap").html('<iframe width="800" height="400" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"'
		+'src="/web_contacts/googleMap3.php?site=1&part=4"></iframe><br/>');

//lh = $("#largemap").width();
//$("#largemap").width(300);
//$("#largemap").width(lh);

return false;
}
function showMore() {
if($("#extraButton").val()=="Show more"){
$("#extraButton").val("Show less");
$("#hiddenExtra").show();
}else{
$("#extraButton").val("Show more");
$("#hiddenExtra").hide();
}
}
function loadSite(t,name){
	$('.trainingLinks > a').attr("class","");
	$(t).attr("class","clicked");
	$.post(
		"/client/ajax.php",
		{action:"changeTraining",type:name},
		function(data){
			//alert(data);
			$("#trainingResultBlock").html(data);
		}
	);
	fn = window['ready'+name];
	fnExists = typeof fn === 'function';
	if (fnExists){
		fn();
	}
	return false;
}
</script>
