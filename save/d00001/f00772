                <script type="text/javascript">
                  function txttrs()
                  {
                    var ElementsCount=document.txttrsForm.elements.length;
                    for(i=0;i<ElementsCount;i++) {
                      if (document.txttrsForm.elements[i].name=='txtDirection' && document.txttrsForm.elements[i].checked) {
                        var tl=document.txttrsForm.elements[i].value;
                        document.txttrsForm.SLANG.value = tl.substr(0,2);
                        document.txttrsForm.TLANG.value = tl.substr(2,2);
                        break;
                      }
                    }
                    document.txttrsForm.submit();
                  }
                </script>
