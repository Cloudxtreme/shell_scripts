<SCRIPT TYPE="text/javascript">

function search_submit() {
	
if (document.getElementById('jumpMenu').value=="ThisSite") {
	window.location="/Search/pages/Results.aspx?k=" + document.getElementById('search_field').value;
}
else if (document.getElementById('jumpMenu').value=="AllSite") {
    window.location="/Search/Pages/AutonomySearch.aspx?k=" + document.getElementById('search_field').value;
}
}
</script>


<SCRIPT TYPE="text/javascript">
function click_text() {
	document.getElementById('search_field').value="";
	
}
</SCRIPT>

<SCRIPT TYPE="text/javascript">
function lost_text() {
	if (document.getElementById('search_field').value=="")
	{
		document.getElementById('search_field').value = "Search by keywords"
	}
	
}

</SCRIPT>





	
