<script type="text/javascript" src="javascript/jquery/jquery.cycle.all.js"></script>
<script type="text/javascript">
$(function() {
	$('#s1').cycle({ 
			fx:     'fade', 
			timeout: 2000, 
			timeoutFn: calculateTimeout,
			// reset images so animated gifs start from beginning
			before: function(curr, next, opts) {
				var $next = $('img', next), src = $next.attr('src');
				$next.attr('src', '');
        $next.attr('src', src);
      }
			// reset images - end
	}); 
	$('#s1 img').css("display","block");
});

// timeouts per slide (in seconds)
var timeouts = [3,3,5,3,5,12,3,3,3];

function calculateTimeout(currElement, nextElement, opts, isForward) {
	var index = opts.currSlide;
	return timeouts[index] * 1000;
}
</script>

