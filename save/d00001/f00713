else { d.loadCSS('http://www.griffith.edu.au/__data/assets/css_file/0007/440647/mobilestyleexpandedcss.css?v=1.2'); d.cookieObj.author('device', 'theme-mobile'); head.js("http://www.griffith.edu.au/__data/assets/js_file/0009/440568/core.global.js"); head.js("http://www.griffith.edu.au/__data/assets/js_file/0008/440648/core.mobile.js?v=1.2"); } });
</script>



