<script type="text/javascript">$(document).ready(function(){$presslist=$('#presslist');$li=$presslist.find('li');$.each(["notable","print","web","release","case"],function(){$presslist.data(this+'',$li.filter('.press-'+this));});function show_press_category(press_category){var $new_category=$presslist.data(press_category);if(!$new_category){return;}
var $old_category=$presslist.data('visible-category');$old_category.fadeOut(function(){$old_category.eq(0).removeClass('firstli');$new_category.eq(0).addClass('firstli');$presslist.data('visible-category',$new_category);$new_category.fadeIn();});}
$('#sidebarnav a.show-press').click(function(){var $this=$(this);$presslist.data('active-link').removeClass('active');var category=$this.addClass('active').attr('href').replace(/^[^#]*#/,'');$presslist.data('active-link',$this);show_press_category(category);return false;});$li.hide();var initial_category_name=document.location.hash.replace(/^#/,'');if(!initial_category_name||!$presslist.data(initial_category_name)){initial_category_name='notable';}
var $initial_category=$presslist.data(initial_category_name);$initial_category.eq(0).addClass('firstli');$presslist.data('visible-category',$initial_category);$initial_category.fadeIn();$presslist.data('active-link',$('#sidebarnav a[href$=#'+initial_category_name+']').addClass('active'));});</script>

