					if ( img == "" ) {
						img = "/images/space.gif";
					}
					document.images["weatherImg"].src=img;
					document.images["weatherImg"].alt=alt;
				}
				function setAmedasLink(level, id, imgKind) {
					if ( level == "" || id == "" ) {
						level = "all";
					}
					if ( level == "area" ) {
						//レーダーアメダスページの場合、自ページへのリンクを外す
						if(imgKind == "amedas_temp"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_temp", "/cs/radar-amedas/area-amedas-temp/" + id + "/1.htm");
						}
						if(imgKind == "amedas_rain"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_rain", "/cs/radar-amedas/area-amedas-rain/" + id + "/1.htm");
						}
						if(imgKind == "amedas_wind"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_wind", "/cs/radar-amedas/area-amedas-wind/" + id + "/1.htm");
						}
						if(imgKind == "amedas_sun"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_sun", "/cs/radar-amedas/area-amedas-sun/" + id + "/1.htm");
						}
						if(imgKind == "amedas_radar"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_radar", "/cs/radar-amedas/area-amedas-radar/" + id + "/1.htm");
						}
						if(imgKind == "amedas_eisei"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_eisei", "/amedas_eisei.htm");
						}
						if(imgKind == "amedas_zikkyo"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_zikkyo", "/amedas_zikkyo.htm");
						}
						if(imgKind == "amedas_gousei"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_gousei", "/amedas_gousei.htm");
						}
						if(imgKind == "amedas_yosou"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_yosou", "/amedas_yosou.htm");
						}
					} else {
						if(imgKind == "amedas_temp"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_temp", "/amedas_temp.htm");
						}
						if(imgKind == "amedas_rain"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_rain", "/amedas_rain.htm");
						}
						if(imgKind == "amedas_wind"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_wind", "/amedas_wind.htm");
						}
						if(imgKind == "amedas_sun"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_sun", "/amedas_sun.htm");
						}
						if(imgKind == "amedas_eisei"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_eisei", "/amedas_eisei.htm");
						}
						if(imgKind == "amedas_zikkyo"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_zikkyo", "/amedas_zikkyo.htm");
						}
						if(imgKind == "amedas_gousei"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_gousei", "/amedas_gousei.htm");
						}
						if(imgKind == "amedas_yosou"){
							setNoLink(imgKind);
						} else {
							setLink("amedas_yosou", "/amedas_yosou.htm");
						}

						setLink("amedas_radar", "/cs/radar-amedas/area-amedas-radar/3/1.htm");
					}
				}
				function setLink(linkId, linkUrl){
					var baseTag = document.getElementById(linkId);
					var link1 = baseTag.firstChild.nodeValue;
					var aTag = document.createElement("a");
					aTag.href = linkUrl;
					aTag.appendChild(document.createTextNode(link1));
					baseTag.replaceChild(aTag, baseTag.firstChild);
				}
				function setNoLink(linkId){
					var baseTag = document.getElementById(linkId);
					baseTag.className = "select";
				}

			//-->
		</script>
