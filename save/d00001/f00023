<script type="text/javascript"> 
function checkForm(frm){ 
msg = ""; 
   if(!frm._mail_from.value){ 
      msg += "The email field is required \n"; 
      alert(msg);
      return false;
   } 
   else {
   return FrontPage_Form1_Validator(this)
   }
} 
</script>

