<script>
var atual = 1;
var c=0;
var t;
var timer_is_on=1;

function timedCount()
{
	t=setTimeout("timedCount()",8000);
	if(timer_is_on){
		mudaDestaque('nao','prox');
	}
}

function doTimer()
{
	if (timer_is_on)
	{
		/*timedCount();*/
		t=setTimeout("timedCount()",6000);
	}
}

doTimer();

function playStop(){

	if(timer_is_on == 1){
		timer_is_on = 0;
	}else{
		timer_is_on = 1;
	}

}
function stop(){timer_is_on = 0;}
function play(){timer_is_on = 1;}

function mudaDestaque(dest,aux){

	if(dest != 'nao')
		stop();

	var anterior = atual;

	if(aux == 'prox'){
		atual += 1;
		if(atual >4)
			atual = 1;
	}else if(aux == 'ant'){
		atual -= 1;
		if(atual < 1)
			atual = 4;
	}else{
		atual = dest;
	}
	jQuery("#destaque_centro_"+anterior+" img").fadeOut(1500);
	jQuery("#destaque_centro_"+anterior).fadeOut(1500, function() {
		jQuery("#destaque_centro_"+atual+" img").hide();
		jQuery("#destaque_centro_"+atual+" img").fadeIn(1500);
		jQuery("#destaque_centro_"+atual).fadeIn(1500);
	});


	jQuery("#pag_1,#pag_2,#pag_3,#pag_4,").removeClass("ativo");
	jQuery("#pag_"+atual).addClass("ativo");

}
</script>

