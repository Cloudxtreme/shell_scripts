<script language="JavaScript">
var sTargetURL = "index.html";
function doRedirect()
{
    setTimeout( "window.location.href = sTargetURL", 2*500 );
    
}
</script>

<script language="JavaScript1.1">

function doRedirect()
{
  
    window.location.replace( sTargetURL );
}

doRedirect();

</script>

</head>

<body onload="doRedirect()">

