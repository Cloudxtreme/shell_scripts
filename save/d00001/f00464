<!--Image File-->
              <td colspan="2" height="180" align="center">
                <script type="text/javascript"><!--
                var img1=new Array("images/spring1_e.jpg","images/spring2_e.jpg","images/spring3_e.jpg","images/spring4_e.jpg");
                var img2=new Array("images/summer1_e.jpg","images/summer2_e.jpg","images/summer3_e.jpg","images/summer4_e.jpg");
                var img3=new Array("images/autumn1_e.jpg","images/autumn2_e.jpg","images/autumn3_e.jpg","images/autumn4_e.jpg");
                var img4=new Array("images/winter1_e.jpg","images/winter2_e.jpg","images/winter3_e.jpg","images/winter4_e.jpg");
ok();
                  document.write('<table border="0"><tr><td><a href="postcard_e.html" target="_top"><img src="'+image1+'" alt="" border="0"></a>');
                  document.write('</td><td></td><td>');
                  document.write('<a href="postcard_e.html" target="_top"><img src="'+image2+'" alt="" border="0"></a></td></tr></table>');
                function ok(){
                  var myMonth=new Date();
                  myDate = myMonth.getMonth() + 1;
                  if(myDate>=3 && myDate<6){    //spring:3-5
                    image1 = img1[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img1[urli2=Math.floor(Math.random()*100) % img1.length];
                  }else if(myDate>=6 && myDate<9){  //summer:6-8
                    image1 = img2[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img2[urli2=Math.floor(Math.random()*100) % img1.length];
                  }else if(myDate>=9 && myDate<12){  //autumn:9-11
                    image1 = img3[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img3[urli2=Math.floor(Math.random()*100) % img1.length];
                  }else if(myDate==12 || myDate<=2){  //winter:12-2
                    image1 = img4[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img4[urli2=Math.floor(Math.random()*100) % img1.length];
                  }
                  if(urli1==urli2)ok();
                }
                //--></script>
              </td>
<!--Image File End-->

