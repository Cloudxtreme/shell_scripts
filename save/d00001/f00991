<script type="text/javascript">
function setParam(node, fi) {
    var id = node.id; 
    if(isNaN(id)){
        id = 0;
    }
    fi.loader.dataUrl = "/content/mice/tag/tagSearch.taxonomy.json?nodeId="+ id + "&path=/content/mice/en";
}

</script> 





<script type="text/javascript">



function openpopupwindow() 
{ 

	var result_openpopup="";
	var result_ind="";
	
	var url_counter ="/content/mice/en/jcr%3Acontent.browse_template_2.xyz";
	$.get(url_counter,{},
	   function(data){
	   		document.getElementById('realAjexContent_counter').innerHTML = data;
	   		result_openpopup = document.getElementById('openpopup_xyz').value;
 			result_ind = document.getElementById('ind_xyz').value;
 			
 			if("true"==result_openpopup)
			{
				jQuery.facebox({ ajax: '/content/cqform/popup_window/popup_window.mice.en.html?survey=1&ind='+result_ind});
			}
	   }
	);	
				
	result_openpopup="";
	result_ind="";
		
} 
</script>

