$(document).ready(function() {
                var showFlash = true;
                
                if(ie != 0)
                                showFlash = true;
                
                if(showFlash){
                                var flashvars = {};
                                flashvars.config = "/content/dam/mice/en/homepage/xml/homeConfig.xml";
                                var params = {};
                                params.menu = "false";
                                params.wmode = "transparent";
                                params.bgcolor = "#000000";
                                var attributes = {};
                                attributes.id = "mainBanner";
                                swfobject.embedSWF("/content/dam/mice/en/homepage/swf/home.swf", "mainBanner", "960", "405", "9.0.0", "http://www.yoursingapore.com/content/dam/mice/en/homepage/swf/expressInstall.swf", flashvars, params, attributes);         
                }
                
                // --- check if mobile device ---
                /*
                if(is_mobile()){
                                showFlash = false
                }
                */
                
                
});

function is_mobile() {
    var agents = ['android', 'webos', 'iphone', 'ipad', 'blackberry'];
    for(i in agents) {
        if(navigator.userAgent.match('/'+agents[i]+'/i')) {
            return true;
        }
    }
    return false;
}
</script>
    
