<script type="text/javascript" src="http://f.tuesday.cz/webtemp/jsloader-8d929932a523-waypoints.min.js?t=1360592692"></script>
<script type="text/javascript" src="http://f.tuesday.cz/webtemp/jsloader-7a0c1e9b700d-waypoints-sticky.min.js?t=1360592692"></script>
<script type="text/javascript" src="http://f.tuesday.cz/webtemp/jsloader-f0b937be48a6-jcarousellite.min.js?t=1360592691"></script>


			
		<script type="text/javascript">
		

		$(document).ready(function() {
			$('.membershipSelection').waypoint('sticky');

			$('.companyMembersLinks').waypoint(function(event, direction) {
    			$('.membershipSelection').toggleClass('showDoodle');
  			}, {offset: 180});
		});
		





		var joinUsPromoHeightHandler = function () {
			var viewPortHeight = $(window).height();
			var logosHeight = $('.ourMembers #partnerLogos').height();
			var ourMembersHeight = $('.ourMembers').height();
			var membershipSelectionHeight = $('.membershipSelection').height();
			var joinUsPromoHeight = viewPortHeight - membershipSelectionHeight ;

			if ((viewPortHeight > ourMembersHeight)&&(joinUsPromoHeight < 340) ) {
				$('.joinUsImage').height(joinUsPromoHeight);
				$('.joinUsImageHolder').height(joinUsPromoHeight);
			}
			if (viewPortHeight > ourMembersHeight) {
				$('.joinUsImageHolder').height(joinUsPromoHeight);
				$('.joinUsImageHolder').css( "line-height",joinUsPromoHeight+"px" );

				if (joinUsPromoHeight < 340) {$('.joinUsImage').height(joinUsPromoHeight-80);}

			}
		}

		joinUsPromoHeightHandler();

		$(function() {
   			 $(".eventGallery .fullView").jCarouselLite({
       		 	btnNext: ".next",
       		 	btnPrev: ".prev",
       		   	auto: 1600,
		    	speed: 1000
    		});
		});

		</script>



		
		


