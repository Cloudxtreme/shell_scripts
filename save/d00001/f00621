<script language="javascript" type="text/javascript">
var timeout         = 500;
var closetimer		= 0;
var ddmenuitem      = 0;
var global_mainlabel_item = 0;
var global_page_main = '';
var global_page_mopenid = 'x';

function homenav_m_over(mainlabel_id, homepage_flag) {
	// cancel close timer
	mcancelclosetime();

	// close old layer
	if(ddmenuitem) {
		ddmenuitem.style.visibility = 'hidden';
		new_bgcol = '#FFFFFF';
		new_fontcol = '#000000';
		if (global_page_mopenid == global_page_main) {  // value for id: m1,m2,m3 etc
			// new_bgcol = '#FFFFFF';//control the color of Home after mouse over it
			new_fontcol = '#FF3399';
		}
		global_mainlabel_item.style.backgroundColor = new_bgcol;
		global_mainlabel_item.style.color = new_fontcol;
	}

	mainlbl_div = document.getElementById(mainlabel_id);
	new_bgcol = '#FF3399';
	new_fontcol = '#FFFFFF';
	mainlbl_div.style.backgroundColor = new_bgcol;
	mainlbl_div.style.color = new_fontcol;
}

function homenav_m_out(mainlabel_id, homepage_flag) {
	mainlbl_div = document.getElementById(mainlabel_id);
	new_bgcol = '#FFFFFF';
	new_fontcol = '#000000';
	if (homepage_flag == '1') {  // homepage is selected - i.e. index.php
		//new_fontcol = '#FF3399';
	}
	mainlbl_div.style.backgroundColor = new_bgcol;
	mainlbl_div.style.color = new_fontcol;
}

// open hidden layer
function mopen(id, mainlabel_id)
{
	// cancel close timer
	mcancelclosetime();

	// close old layer
	if(ddmenuitem) {
		ddmenuitem.style.visibility = 'hidden';
		new_bgcol = '#FFFFFF';
		new_fontcol = '#000000';
		if (global_page_mopenid == global_page_main) {  // value for id: m1,m2,m3 etc
			// new_bgcol = '#FFFFFF';
			new_fontcol = '#FF3399';
		}
		global_mainlabel_item.style.backgroundColor = new_bgcol;
		global_mainlabel_item.style.color = new_fontcol;
	}

	global_page_mopenid = id;

	// get new layer and show it
	ddmenuitem = document.getElementById(id);
	ddmenuitem.style.visibility = 'visible';
	mainlabel_item = document.getElementById(mainlabel_id);
	global_mainlabel_item = mainlabel_item;
	mainlabel_item.style.backgroundColor = '#FF3399';
	mainlabel_item.style.color = '#FFF';
}
// close showed layer
function mclose()
{
	if(ddmenuitem) {
		ddmenuitem.style.visibility = 'hidden';
		new_bgcol = '#FFFFFF';
		new_fontcol = '#000000';
		if (global_page_mopenid == global_page_main) {  // value for id: m1,m2,m3 etc
			// new_bgcol = '#FFFFFF';
			new_fontcol = '#FF3399';
		}
		global_mainlabel_item.style.backgroundColor = new_bgcol;
		global_mainlabel_item.style.color = new_fontcol;
	}
}

// go close timer
function mclosetime()
{
	closetimer = window.setTimeout(mclose, timeout);
}

// cancel close timer
function mcancelclosetime()
{
	if(closetimer)
	{
		window.clearTimeout(closetimer);
		closetimer = null;
	}
}

function gup( name )
{
  name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
  var regexS = "[\\?&]"+name+"=([^&#]*)";
  var regex = new RegExp( regexS );
  var results = regex.exec( window.location.href );
  if( results == null )
    return "";
  else
    return results[1];
}



// close layer when click-out
document.onclick = mclose; 

function onloadfn() {

	getMe();

}

function getMe() {
var imgshow = gup('id');
var stringdisplay = "";
if(imgshow == ""){
	stringdisplay = '<iframe allowtransparency="true" src="home.htm" width="1021px" frameborder="0" scrolling="auto" height="528px"></iframe>';
}
else {
	stringdisplay ='<img src="screenshots/' + imgshow + '.png" alt="" width="1000px" />';
}

//alert(stringdisplay);
displayimg(stringdisplay);

}


function displayimg(mystring) {
	document.getElementById('screenshot').innerHTML = mystring;
}



//Navigation Highlight
function colorAboutus(){
if(document.URL.indexOf("AboutUs")>0){
global_page_main = 'm1';
document.getElementById("lbl_main01").style.color="#FF0099";
document.getElementById("lbl_main01").style.fontWeight="900";
document.getElementById("lbl_main01").style.padding="4px 12px";
}
else if(document.URL.indexOf("Industry")>0){
global_page_main = 'm2';
document.getElementById("lbl_main02").style.color="#FF0099";
document.getElementById("lbl_main02").style.fontWeight="900";
document.getElementById("lbl_main02").style.padding="4px 12px";
}
else if(document.URL.indexOf("Public")>0){
global_page_main = 'm3';
document.getElementById("lbl_main03").style.color="#FF0099";
document.getElementById("lbl_main03").style.fontWeight="900";
document.getElementById("lbl_main03").style.padding="4px 12px";
}
else if(document.URL.indexOf("International")>0){
global_page_main = 'm4';
document.getElementById("lbl_main04").style.color="#FF0099";
document.getElementById("lbl_main04").style.fontWeight="900";
document.getElementById("lbl_main04").style.padding="4px 12px";
}
else if(document.URL.indexOf("Policies")>0){
global_page_main = 'm5';
document.getElementById("lbl_main05").style.color="#FF0099";
document.getElementById("lbl_main05").style.fontWeight="900";
document.getElementById("lbl_main05").style.padding="4px 12px";
}
else if(document.URL.indexOf("Licences")>0){
global_page_main = 'm6';
document.getElementById("lbl_main06").style.color="#FF0099";
document.getElementById("lbl_main06").style.fontWeight="900";
document.getElementById("lbl_main06").style.padding="4px 12px";
}
else if(document.URL.indexOf("Schemes")>0){
global_page_main = 'm7';
document.getElementById("lbl_main07").style.color="#FF0099";
document.getElementById("lbl_main07").style.fontWeight="900";
document.getElementById("lbl_main07").style.padding="4px 12px";
}
else if(document.URL.indexOf("Reports")>0){
global_page_main = 'm8';
