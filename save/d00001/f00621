<script>

function openwindow(url) {
	window.open(url);
}



function showalldiv(){
	var x = document.getElementsByTagName('div');
	for (var i=0;i<x.length;i++)
	{
			
			if (x[i].id.indexOf("iv_") > 0) x[i].style.display = '';
			
	}

}
function hidealldiv(){
	var x = document.getElementsByTagName('div');
	for (var i=0;i<x.length;i++)
	{
			if (x[i].id.indexOf("iv_") > 0) x[i].style.display = 'none';
			
	}

}



var IFrameObj; // our IFrame object
function callToServer(ptype, pfaqsysid, pScore) {

	
  if (!document.createElement) {return true};
  var IFrameDoc;
  var URL;
  if(ptype=='RATINGS'){
  	var searchphrase;
  	searchphrase='';
  	if (document.forms[0].txtKeyword != null) {
  		searchphrase = searchphrase + ' ' + document.forms[0].txtKeyword.value;
  	}
  	
  	if (document.forms[0].txtBoolean_All != null) {
  		searchphrase = searchphrase + ' ' + document.forms[0].txtBoolean_All.value;
  	}
  	
  	if (document.forms[0].txtBoolean_exact != null) {
  		searchphrase = searchphrase + ' ' + document.forms[0].txtBoolean_exact.value;
  	}
  	
  	if (document.forms[0].txtBoolean_OR != null) {
  		searchphrase = searchphrase + ' ' + document.forms[0].txtBoolean_OR.value;
  	}
  	
  	if (document.forms[0].txtBoolean_exclude != null) {
  		searchphrase = searchphrase + ' ' + document.forms[0].txtBoolean_exclude.value;
  	}
  	
  	//URL = 'fcd_updatehits.aspx?_strFAQSysID=' + pfaqsysid + '&_strpType=' + ptype + '&_pScore=' + pScore + '&_strKeyword=' + encodeURIComponent(document.forms[0].txtKeyword.value);
  	URL = 'fcd_updatehits.aspx?_strFAQSysID=' + pfaqsysid + '&_strpType=' + ptype + '&_pScore=' + pScore + '&_strKeyword=' + encodeURIComponent(searchphrase);
  }
  else{
   	URL = 'fcd_updatehits.aspx?_strFAQSysID=' + pfaqsysid + '&_strpType=' + ptype;
  }
 
  ////////////////////////////
   	//call xmlhttp	
	var blnxml_flag;
	blnxml_flag = loadXMLDoc(URL);
	///////////////////
 
	if (!blnxml_flag) { 	
 
	  if (!IFrameObj && document.createElement) {
	    // create the IFrame and assign a reference to the
	    // object to our global variable IFrameObj.
	    // this will only happen the first time 
	    // callToServer() is called
	   try {
	      var tempIFrame=document.createElement('iframe');
	      tempIFrame.setAttribute('id','RSIFrame');
	      tempIFrame.style.border='0px';
	      tempIFrame.style.width='0px';
	      tempIFrame.style.height='0px';
	      IFrameObj = document.body.appendChild(tempIFrame);
	      
	      if (document.frames) {
	        // this is for IE5 Mac, because it will only
	        // allow access to the document object
	        // of the IFrame if we access it through
	        // the document.frames array
	        IFrameObj = document.frames['RSIFrame'];
	      }
	    } catch(exception) {
	      // This is for IE5 PC, which does not allow dynamic creation
	      // and manipulation of an iframe object. Instead, we'll fake
	      // it up by creating our own objects.
	      iframeHTML='\<iframe id="RSIFrame" style="';
	      iframeHTML+='border:0px;';
	      iframeHTML+='width:0px;';
	      iframeHTML+='height:0px;';
	      iframeHTML+='"><\/iframe>';
	      document.body.innerHTML+=iframeHTML;
	      IFrameObj = new Object();
	      IFrameObj.document = new Object();
	      IFrameObj.document.location = new Object();
	      IFrameObj.document.location.iframe = document.getElementById('RSIFrame');
	      IFrameObj.document.location.replace = function(location) {
	        this.iframe.src = location;
	      }
	    }
	  }
	  
	  if (navigator.userAgent.indexOf('Gecko') !=-1 && !IFrameObj.contentDocument) {
	    // we have to give NS6 a fraction of a second
	    // to recognize the new IFrame
	    setTimeout('callToServer()',10);
	    //return false;
	  }
	  
	  if (IFrameObj.contentDocument) {
	    // For NS6
	    IFrameDoc = IFrameObj.contentDocument; 
	  } else if (IFrameObj.contentWindow) {
	    // For IE5.5 and IE6
	    IFrameDoc = IFrameObj.contentWindow.document;
	  } else if (IFrameObj.document) {
	    // For IE5
	    IFrameDoc = IFrameObj.document;
	  } else {
	   // return true;
	  }
	  
	  IFrameDoc.location.replace(URL);
	  //return false;
	}
		  
  	if(ptype=='RATINGS'){
  	alert('Thank you for your input.  We will use your input to refine future searches.');
  	}
}

function loadXMLDoc(url){

 xmlhttp=null;
 try {
 xmlhttp=new ActiveXObject("Msxml2.XMLHTTP");
 }
 catch(e) {
 try {
 xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
 }
 catch(oc) {
 xmlhttp=null;
 }
 }

 if ( !xmlhttp && typeof XMLHttpRequest != "undefined" ) {
 xmlhttp=new XMLHttpRequest();
 }


  	
	if (xmlhttp!=null){
  		xmlhttp.onreadystatechange=state_Change;
  		xmlhttp.open("GET",url,true);
  		xmlhttp.send(null);
  		return true;
  	}
	else{
  		alert("Your browser does not support XMLHTTP.");
  		return false;	
  	}
}








function loadXMLDoc1(url){
	xmlhttp=null
	// code for Mozilla, etc.
	if (window.XMLHttpRequest){
  		xmlhttp=new XMLHttpRequest();
  	}
	// code for IE
	else if (window.ActiveXObject){
  		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  	}
	if (xmlhttp!=null){
  		xmlhttp.onreadystatechange=state_Change;
  		xmlhttp.open("GET",url,true);
  		xmlhttp.send(null);
  		loadXMLDoc == true;
  	}
	else{
  		alert("Your browser does not support XMLHTTP.");
  		loadXMLDoc == true;	
  	}
}
	
function state_Change(){
	// if xmlhttp shows "loaded"
	if (xmlhttp.readyState==4){
	  	// if "OK"
	  	if (xmlhttp.status==200){
	  		//alert('ok');
	    }
	  	else{
	    	//alert("Problem retrieving XML data")
	    }
  	}
}


//added 23 Mar 2008
function generate_search_string(strquerystring){
	var strurl;
	var strvalue;
	strvalue ="";
	
	if (document.forms[0].txtKeyword != null){
		if (document.forms[0].txtKeyword.value != null){
			strvalue = strvalue + document.forms[0].txtKeyword.value;
		}	
	}
	
	if (document.forms[0].txtBoolean_All != null){
		if (document.forms[0].txtBoolean_All.value != ''){
		strvalue = strvalue + '&q1=' + document.forms[0].txtBoolean_All.value;
		}
	}
	
	if (document.forms[0].txtBoolean_exact != null){
		if (document.forms[0].txtBoolean_exact.value != ''){
			strvalue = strvalue + '&q2=' + document.forms[0].txtBoolean_exact.value;
		}
	}
	
	if (document.forms[0].txtBoolean_OR != null){
		if (document.forms[0].txtBoolean_OR.value != ''){
			strvalue = strvalue + '&q3=' + document.forms[0].txtBoolean_OR.value;
		}
	}
	
	if (document.forms[0].txtBoolean_exclude != null){
		if (document.forms[0].txtBoolean_exclude.value != ''){
			strvalue = strvalue + '&q4=' + document.forms[0].txtBoolean_exclude.value;
		}
	}
	
	//amended 6 Aug 2009
	if (document.forms[0].m_strSearch_Choice_2 != null){
		if (document.forms[0].m_strSearch_Choice_2.checked==true){
			strvalue = strvalue + '&searchchoice=' + document.forms[0].m_strSearch_Choice_2.value;
		}
		else if(document.forms[0].m_strSearch_Choice_1 .checked==true){
			strvalue = strvalue + '&searchchoice=' + document.forms[0].m_strSearch_Choice_1.value;
		}
	}
	else if (document.forms[0].m_strSearch_Choice_1 != null){
		if(document.forms[0].m_strSearch_Choice_1.checked==true){
			strvalue = strvalue + '&searchchoice=' + document.forms[0].m_strSearch_Choice_1.value;
		}
	}
	
	//amended 6 Aug 2009
	
	if (document.forms[0].txtKeyword != null){
		strurl = '../apps/fcd_faqmain.aspx?qst=' + strquerystring + '&q=' + strvalue + '&keyword='+ document.forms[0].txtKeyword.value + '&_intPage=' + document.forms[0].m_intPage.value;
	}
	else{
		strurl = '../apps/fcd_faqmain.aspx?qst=' + strquerystring + '&q=' + strvalue + '&_intPage=' + document.forms[0].m_intPage.value;
	}	
	
	
	window.open(strurl);
}

function bookmark(url, title) {

    if ((navigator.appName == "Microsoft Internet Explorer") && (parseInt(navigator.appVersion) >= 4)) {
        window.external.AddFavorite(url, title);
    }
    //else if (navigator.appName == "Netscape") {
    //window.sidebar.addPanel(title,url,"",true);
    //} 
    else {
        alert("Press CTRL-D (Netscape and Firefox) or CTRL-T (Opera) to bookmark");
    }
}


