        <span class="more" id="more">Více</span>

		<script type="text/javascript">
			(function() {
				var openMenu = 0;
				var hideTimer;
				var hideMenuFunc = function() {
					$("#database ul").css("overflow","hidden").css("height", "45px");
					openMenu = 0;
				};
				$("#database").hover(
					function() {
						if (openMenu && hideTimer) {
							clearTimeout(hideTimer);
							hideTimer = null;
						}
					},
					function() {
						if (openMenu) {
							hideTimer = setTimeout(hideMenuFunc, 300);
						}
					}
				);
				$("#more").mouseover(
					function() {
						$("#database ul").css("overflow", "visible").css("height", "90px");
						openMenu = 1;

					}
				);
			})();
        </script>

</div>

