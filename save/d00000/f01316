    <script type="text/javascript">
  $(document).ready(function(){
    var now = new Date();
    var yesterday = new Date();
    yesterday.setDate(now.getDate()-1)
    $("li.appointmentItem .date").each(function(){
      var parsed_date = $.datepicker.parseDate('dd.mm.yy', $(this).text());
      if (+parsed_date < +yesterday) {
        $(this).parent().remove();
      }
    });
  });
</script>

