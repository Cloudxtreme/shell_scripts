	var servidor = "http://netdeb.pop-es.rnp.br";
	var GB_ROOT_DIR = "./greybox/greybox/";
    function gerapage(){
	    var i;
	    for (i=0;i<document.formip.function.length;i++){
		    if (document.formip.function[i].checked)
			    break;		           
	    }
	    var a= servidor + '/rede.php?host=' + document.formip.host.value + '&function=' + document.formip.function[i].value;
	    for (i=0;i<document.formip.digop.length;i++){
		    if (document.formip.digop[i].checked)
			    a = a + '&digop[]=' + document.formip.digop[i].value;
	    }
	    return a; 
    }
    function whois(ip){
	    var i; 
	    var a= servidor + '/rede.php?host=' + ip + '&function=whois';
	    return a; 
    }
</script>
