<script type="text/javascript">
$(document).ready(function() {
	$('#register').attr('disabled', 'disabled');
	$('select#country').change(function() {
		$(this).next('span').removeClass().text('');
		if($(this).val() == 'US') {
			$('#otherstate').hide();
			$('#otherstate * input').attr('disabled', 'disabled');
			$('#usstate').show();
			$('#usstate * select').removeAttr('disabled');
		} else if($(this).val() == '') {
			$(this).next('span').addClass('fieldError').text('Country required');
			$('#usstate').hide();
			$('#usstate * select').attr('disabled', 'disabled');
			$('#otherstate').show();
			$('#otherstate * input').removeAttr('disabled');
		} else {
			$('#usstate').hide();
			$('#usstate * select').attr('disabled', 'disabled');
			$('#otherstate').show();
			$('#otherstate * input').removeAttr('disabled');
		}
	});
	$('select#state').change(function() {
		$(this).next('span').removeClass().text('');
		if($(this).val() == '') {
			$(this).next('span').addClass('fieldError').text('State required');
		}
	});
	$('input[needed],select[needed]').each(function() {
		if($(this).val()) {
			var obj = $(this);
			var r = 1;
			$.post('/register.php?ajax=true', { field: obj.attr('name'), value: obj.val() }, function(data) {
				if(data.length > 0) {
					obj.next('span').addClass('fieldError').text(data);
					$('#register').attr('disabled', 'disabled');
				} else {
					obj.next('span').removeClass().text('');
				}
			}, 'text');
		} else {
			$(this).next('span').removeClass().text('');
			$('#register').attr('disabled', 'disabled');
		}
	});
	$('input[needed],select[needed]').change(function() {
		if($(this).val()) {
			var obj = $(this);
			$.post('/register.php?ajax=true', { field: obj.attr('name'), value: obj.val() }, function(data) {
				if(data.length > 0) {
					obj.next('span').addClass('fieldError').text(data);
				} else {
					obj.next('span').removeClass().text('');
				}
				var r = 0;
				$('input[needed],select[needed]').each(function() {
					if($(this).next('span').hasClass('fieldError') && $(this).is(':visible')) {
						r = 1;
					}
				});
				if((r == 0) && $('#tos').is(':checked')) {
					$('#register').removeAttr('disabled');
				} else {
					$('#register').attr('disabled', 'disabled');
				}
			}, 'text');
		} else {
			$(this).next('span').removeClass().text('');
			$('#register').attr('disabled', 'disabled');
		}
	});
});
</script>


