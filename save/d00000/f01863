<script  type="text/javascript">
    function oneRadioButtonSelected (chkVal, idVal) 
	{
	    //alert(idVal);
	    //document.write(idVal);
	    
	    idValFirstPart = idVal.substring(0, 45); // Poll_rptPollInstancesForVote__ctl0_rptAnswers__ctl6_rbOption -> Poll_rptPollInstancesForVote__ctl0_rptAnswers
	    //alert(idValFirstPart);
	    
	    //alert(chkVal);
	    
        var frm = document.forms[0];
        // loop through all elements
        for (i=0; i<frm.length; i++) 
        {
            //alert(frm.elements[i].id);
            //alert(frm.elements[i].id.indexOf(idVal) != -1);
            if (frm.elements[i].id.indexOf(idValFirstPart) != -1) 
            {
                //alert(frm.elements[i].id);
                
                if (frm.elements[i].id.indexOf(idVal) != -1)
                    frm.elements[i].checked = chkVal;
                else
                    frm.elements[i].checked = false;
            }
        }
    }
</script>

