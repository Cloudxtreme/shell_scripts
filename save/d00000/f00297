<script type="text/javascript">
$(document).ready(function(){
    initialize();
    if(window.location.hash) {
        _hashArr = window.location.hash.slice(1).split('/');
        if (_hashArr[0] == 'cat') {
            _categoryVal = '/sites/all/themes/singTelRed/its_location.php?type=store&cat=' + _hashArr[1];
            $('#category').val(_categoryVal);
            $('#category').change();
        } else if (_hashArr[0] == 'product') {
            _productVal = '/sites/all/themes/singTelRed/its_location.php?type=centre&cat=equip&product=' + _hashArr[1];
            $('#product').val(_productVal);
            $('#product').change();
        }
    }
});

<!-- create function to call ajaxcombo2 and clear subdd2 -->
function clearResult(el){
  $("#"+el).html("");
}

function clearBrand(){
  $("#subddc").html("");
}
</script>
	
