  <div class="thumblist"> 
   <a href="thumbsprev" class="nav prev" ts.idz="gal_prv_1"><span class="arrow" rel="prev">&lt;</span></a> 
   <ul> 
    <li id="thumb_b48b8cf8bce8e310VgnVCM4000009bcceb0aRCRD_0ef6cd5db848e310VgnCLD2000000ec6eb0aRCRD" itemscope="" itemtype="http://schema.org/ImageObject"> <a href="http://p1-cdn.trrsf.com/image/fget/cf/619/464/images.terra.com/2013/05/10/1linkedin10-anosdiv.jpg" data-thumb="http://p1-cdn.trrsf.com/image/fget/cf/67/51/images.terra.com/2013/05/10/1linkedin10-anosdiv.jpg" data-copyright="Foto: Divulga&ccedil;&atilde;o" itemprop="contentUrl" ts.idz="gal_thb_1"> 
