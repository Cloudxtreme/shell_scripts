	<script type="text/javascript">
		function modifyParam(key, value){ 
		    var kvp = document.location.search.substr(1).split('&'); 
		    var i = kvp.length; 
		    while ( i-- ) { 
		        var par = kvp[i].split('='); 
		        if ( par[0].toLowerCase() == key ) { // found parameter 
		                par[1] = value; 
		                kvp[i] = par.join('='); 
		                break; 
		        } 
		    } 
		    if ( i < 0 ) { kvp[kvp.length] = [key,value].join('='); } // Param not found, add to the end
		    document.location.search = kvp.join('&');  
		} 

		function pageChange(qtd) {
			var currPage = 1 ;
			modifyParam('vgnpage', currPage+qtd);
		}
	</script><noscript><p>inclus√£o de arquivo javascript</p></noscript>
		

