<SCRIPT LANGUAGE="JavaScript">
<!--- To hide JavaScript from old browsers
var delay = 0 // ページが変わるまでの時間（１０００分の１秒単位）

function nextPage() {
timerId = setTimeout("changePage()", delay)
}

function changePage() {
location = "http://tokai.jp/" // 移動先のファイル名を入れる
}
// --->
</SCRIPT>

<!--JavaScript をサポートしてないブラウザのために refresh を使用して 5秒後に移動-->
<noscript>
<meta http-equiv="refresh" content="0; URL="http://tokai.jp/">
</noscript>

<!-- JavaScriptが有効なら 3秒後に移動 -->

<body onLoad="nextPage()" >

<noscript>
<!-- JavaScript無効時のメッセージ -->
<p><font color="#000000" size=-1>
自動ジャンプしない場合は、<A HREF="http://tokai.jp/">こちら</A>
</font></p>
</noscript>