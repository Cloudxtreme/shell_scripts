function setUpPage() {
globalNav_on = 'st_main_button';


if (globalNav_on != '') {
	hilite(globalNav_on, 1);
}
if (mainNav_on != '') {
	hiliteNavMain(mainNav_on, 1);
}
if (tier1Nav_on != '') {
	hiliteNavLeft(tier1Nav_on, 1, 1, 1);
}
if (tier2Nav_on != '') {
	hiliteNavLeft(tier2Nav_on, 1, 2, 1);
}	
if (subPageNav_on != '') {
	hiliteNavSub(subPageNav_on, 1);
}
} // /setUpPage()
</script>
<script src="/js_css/students_promo.js" type="text/javascript"></script>
<script language="javascript">
var SCEMSPromo1 = '';
var SCEMSPromo2 = '';

var maxMEDPromo = (randPromoMED.length);

var defaultMEDPromo1 = '';
defaultMEDPromo1 = 


'<table width="190" cellspacing="0" cellpadding="0"><tr><td width="190" align="left" valign="top"><a href="/educators/lesson_plans/how_things_fly/index.html"><img src="/images/global/promos/medium/medium_lp_how_fly.jpg" border="0" alt="How Things Fly, Lesson Plan" /></a></td></tr></table>'



;

var defaultMEDPromo2 = '';
defaultMEDPromo2 = 


'<table width="190" cellspacing="0" cellpadding="0"><tr><td width="190" align="left" valign="top"><a href="/contest/winners.htm"><img src="/images/global/promos/large/Promo_large_kids_essay_cont.jpg" border="0" alt="Essay Contest Winners" /></a></td></tr></table>'



;

function writeMEDPromo() {
	SCEMSPromo1 = '';
	var randMEDAccess = Math.floor(((Math.random()*1000000)%maxMEDPromo));
	//alert(maxMEDPromo + ' - ' + randMEDAccess);
	SCEMSPromo1 += randPromoMED[randMEDAccess];
	if ( (parseInt(randMEDAccess) + 1) == maxMEDPromo ) {
		randMEDAccess = -1;
	}
	if (maxMEDPromo == 1) {
		SCEMSPromo2 += defaultMEDPromo2; 
	}
	else {
		SCEMSPromo2 += randPromoMED[parseInt(randMEDAccess) +1];
	}
}

if ((maxMEDPromo > 0)) {
	writeMEDPromo();
}

else if ((maxMEDPromo == 0)) {
	SCEMSPromo1 += defaultMEDPromo1;
	SCEMSPromo2 += defaultMEDPromo2; 
}

</script>
