   	var curSWF;
	jQuery(document).ready(function() {
		if(jQuery.cookie("prflang")) {
			var v = jQuery.cookie("prflang");
			if(v == "zh_CN" || v == "zh_HK" || v == "de_DE" || v == "cs_CZ" || v == "ru_RU" || v == "pt_BR" || v == "vi_VN")
				jQuery("#langopt option[value='"+v+"']").attr("selected", "selected");
		}
		jQuery("#langopt").change(function() {
        		var opt = jQuery(this).val();
			jQuery.cookie("prflang",""+opt+"",{expires:1, path: '/'});
			location.reload();
    		});
	});
  	</script>
