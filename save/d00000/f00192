     // Add a script element as a child of the body
     function getDeferredAnalyticsJS() {
        var element = document.createElement("script");
        element.src = "/jscript/analyticsUtils.js";
        element.async = true;
        document.body.appendChild(element);
     }

   $(function() {
        $("#_info_webtolead_form").submit(function () {
           return validateMainContactForm();
        });
        getDeferredAnalyticsJS();
    });

    function validateMainContactForm() {

        var valid = true;

        var emailPattern = new RegExp('.+@.+\\..*');
        var spacesPattern = new RegExp('\\s+');

        var emaillVal = $.trim($('#webtolead_email').val());
        var lastnameVal = $.trim($('#webtolead_lastname').val());
        var companyVal = $.trim($('#webtolead_company').val());

        if ( emaillVal == "" || !emailPattern.test(emaillVal) || spacesPattern.test(emaillVal)) {
            $('#email-error2').show();
            valid = false;
        } else {
            $('#email-error2').hide();
        }

        if (lastnameVal == ""){
            $('#lastname-error2').show();
        } else {
            $('#lastname-error2').hide();
        }

        if (companyVal == ""){
           $('#company-error2').show();
           valid = false;
        } else {
           $('#company-error2').hide();
        }

        if ($('#recaptcha_response_field').val() == "") {
            $('#image-text-error').show();
            valid = false;
        } else {
            $('#image-text-error').hide();
        }

        if (valid) {
            ('#webtolead_email').val(emaillVal);
            ('#webtolead_lastname').val(lastnameVal);
            ('#webtolead_company').val(companyVal);
        }

        return valid;
    }

    $(document).ready(function() {
        $("#int-landing-chat-button").click(function() {
            window.open('https://sales.liveperson.net/hc/12703439/?cmd=file&file=visitorWantsToChat&site=12703439&SESSIONVAR%21skill=Sales-SL-WWW-English&referrer='+escape(document.location),'chat12703439','width=500,height=500,resizable=yes');return false;        });
        $("#_info_webtolead_form").submit(function () {
           return validateMainContactForm();
        });
    });


