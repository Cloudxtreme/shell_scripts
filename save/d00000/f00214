	function show_file2(media_resource_id, file_type, path) {
	   //alert('1234');
	   window.open("/stockexchange_file.php?file_="+media_resource_id+"&file_type_="+file_type,"Image","left=20,top=20,toolbar=no,location=no,menubar=no,resizable=yes,scrollbars=no,width=640,height=480"); 
		return false;	   
	}

	function show_file3(media_resource_id, file_type, path) {
	   window.open("/stockexchange_file.php?file_="+media_resource_id+"&file_type_="+file_type,"Image","left=20,top=20,toolbar=no,location=no,menubar=no,resizable=yes,scrollbars=no,width=640,height=480"); 
		return false;	   
	}
</script>
<script type="text/javascript">
		$(document).ready(function() {
			$('.camera_pag .camera_pag_ul li span').each(function(){
				//if($(this).height() > 25){
					$(this).css('height','40px');					
				//}
				console.log('update height pls');
				if($.browser.version == '7.0') {
					$(this).css('height','37px');
					$(this).css('padding-top','0');	
					$(this).css('top','0');					
				}  
			});	
		   $('.camera_pag .camera_pag_ul li span span').each(function(){
					$(this).css('height','auto');
					if($(this).height() > 25){
						$(this).css('margin-top','5px');					
					}
					console.log('here');
					/*if($.browser.version == '7.0') {
						$(this).css('height','auto');
						$(this).css('margin-top','10px');
					} */
			});			
		});			


	function centerTitle() {
       $('.camera_pag .camera_pag_ul li span span').each(function(){
				$(this).css('height','auto');
				if($(this).height() > 25){
					$(this).css('margin-top','5px');					
				}
				console.log('here');
				/*if($.browser.version == '7.0') {
					$(this).css('height','auto');
					$(this).css('margin-top','10px');
				} */
		});	
 		
    }
    window.onload = centerTitle;	
</script>
