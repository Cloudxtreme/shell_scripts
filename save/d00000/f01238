			var obj = document.getElementById("captcha_field");
			while( obj ) {
				if( obj.nodeName.toLowerCase() == "form" ) {
					original_submit = obj.onsubmit;
					obj.onsubmit = function() {
						if( document.getElementById("captcha_field").value.length != 3 ) {
