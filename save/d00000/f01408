   	(function() {   		
    
    var ANDROID_REDIRECT = '/content/citrix/en_us/mobile/Android.html';
    var BLACKBERRY_REDIRECT = '/content/citrix/en_us/mobile/BlackBerry.html';
    var IOS_REDIRECT = '/content/citrix/en_us/mobile/iPhone.html';
    var IS_EXCLUDED = false;
    var path = '/content/citrix/en_us/support';
    if( !IS_EXCLUDED ) {
	    if( areCookiesEnabled()  ) {
	       if( readCookie(MOBILE_INTERSTITIAL_COOKIE_NAME) == null ) {
		        if( isMobile.iOS() ) {
		            window.location.href = IOS_REDIRECT;
		         } else if( isMobile.BlackBerry() ) {
		            window.location.href = BLACKBERRY_REDIRECT;
		         } else if( isMobile.Android() ) {
		            window.location.href = ANDROID_REDIRECT;
		         }
	       }
	    }
    }
})();
