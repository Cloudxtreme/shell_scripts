function setUpPage() {
globalNav_on = 'fm_main_button';

if (globalNav_on != '') {
	hilite(globalNav_on, 1);
}
if (mainNav_on != '') {
	hiliteNavMain(mainNav_on, 1);
}
if (tier1Nav_on != '') {
	hiliteNavLeft(tier1Nav_on, 1, 1, 1);
}
if (tier2Nav_on != '') {
	hiliteNavLeft(tier2Nav_on, 1, 2, 1);
}
if (subPageNav_on != '') {
	hiliteNavSub(subPageNav_on, 1);
}
}// /setUpPage()
</script>
<script src="/js_css/families_promo.js" type="text/javascript"></script>
<script language="javascript">
var SCEMSPromo1 = '';
var SCEMSPromo2 = '';
var SCEMSPromo3 = '';

var maxMEDPromo = (randPromoMED.length);

var defaultMEDPromo1 = '';
defaultMEDPromo1 = 


'<table width="190" cellspacing="0" cellpadding="0"><tr><td width="190" align="left" valign="top"><a href="/families/at_the_smithsonian/grandparents_guide.html"><img src="/images/global/promos/medium/pr_med_gguide.gif" border="0" alt="Grandparents Guide to the Smithsonian" /></a></td></tr></table>'



;

var defaultMEDPromo2 = '';
defaultMEDPromo2 = 


'<table width="190" cellspacing="0" cellpadding="0"><tr><td width="190" align="left" valign="top"><a href="/students/explore_by_topic/science_nature.html"><img src="/images/global/promos/medium/promo_med_science.jpg" border="0" alt="Explore by Topic: Science" /></a></td></tr></table>'



;

var defaultMEDPromo3 = '';
defaultMEDPromo3 = 


'<table width="190" cellspacing="0" cellpadding="0"><tr><td width="190" align="left" valign="top"><a href="http://www.discoverytheater.org"><img src="/images/global/promos/medium/pr_med_discovery.jpg" border="0" alt="Discovery Theater" /></a></td></tr></table>'



;

function writeMEDPromo() {
	SCEMSPromo1 = '';
	var randMEDAccess = Math.floor(((Math.random()*1000000)%maxMEDPromo));
	//alert(maxMEDPromo + ' - ' + randMEDAccess);
	SCEMSPromo1 += randPromoMED[randMEDAccess];
	if (maxMEDPromo == 1) {
		SCEMSPromo2 += defaultMEDPromo2; 
		SCEMSPromo3 += defaultMEDPromo3; 
	}
	else {
		if (maxMEDPromo == 2) {
			if ( (parseInt(randMEDAccess) + 1) == maxMEDPromo ) {
				randMEDAccess = -1;
			}
			SCEMSPromo2 += randPromoMED[parseInt(randMEDAccess) +1];
			SCEMSPromo3 += defaultMEDPromo3;
		}
		else{
			if ( (parseInt(randMEDAccess) + 1) == maxMEDPromo ) {
				randMEDAccess = -1;
			}
			SCEMSPromo2 += randPromoMED[parseInt(randMEDAccess) +1];
			if ( (parseInt(randMEDAccess) + 2) >= maxMEDPromo ) {
				randMEDAccess = -2;
			}
			SCEMSPromo3 += randPromoMED[parseInt(randMEDAccess) +2];		
		} 
	}
}

if ((maxMEDPromo > 0)) {
	writeMEDPromo();
}

else if ((maxMEDPromo == 0)) {
	SCEMSPromo1 += defaultMEDPromo1;
	SCEMSPromo2 += defaultMEDPromo2; 
	SCEMSPromo3 += defaultMEDPromo3; 
}

</script>

