<!--↓↓画像ファイルの表示↓↓-->
            <td colspan="2" height="200" align="center">
              <script type="text/javascript"><!--
                var img1=new Array("images/spring1.jpg","images/spring2.jpg","images/spring3.jpg","images/spring4.jpg");
                var img2=new Array("images/summer1.jpg","images/summer2.jpg","images/summer3.jpg","images/summer4.jpg");
                var img3=new Array("images/autumn1.jpg","images/autumn2.jpg","images/autumn3.jpg","images/autumn4.jpg");
                var img4=new Array("images/winter1.jpg","images/winter2.jpg","images/winter3.jpg","images/winter4.jpg");
                ok();
                  document.write('<table border="0"><tr><td><a href="postcard.html" target="_top"><img src="'+image1+'" alt="" border="0"></a>');
                  document.write('</td><td></td><td>');
                  document.write('<a href="postcard.html" target="_top"><img src="'+image2+'" alt="" border="0"></a></td></tr></table>');
                function ok(){
                  var myMonth=new Date();
                  myDate = myMonth.getMonth() + 1;
                  if(myDate>=3 && myDate<6){    //春：3-5月
                    image1 = img1[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img1[urli2=Math.floor(Math.random()*100) % img1.length];
                  }else if(myDate>=6 && myDate<9){  //夏：6-8月
                    image1 = img2[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img2[urli2=Math.floor(Math.random()*100) % img1.length];
                  }else if(myDate>=9 && myDate<12){  //秋：9-11月
                    image1 = img3[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img3[urli2=Math.floor(Math.random()*100) % img1.length];
                  }else if(myDate==12 || myDate<=2){  //冬：12-2月
                    image1 = img4[urli1=Math.floor(Math.random()*100) % img1.length];
                    image2 = img4[urli2=Math.floor(Math.random()*100) % img1.length];
                  }
                  if(urli1==urli2)ok();
                }
              //--></script>
<!--↑↑画像ファイルの表示↑↑-->

