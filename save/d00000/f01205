<article id="post-65" class="post-65 page type-page status-publish hentry">
	<header class="entry-header">
		<h1 class="entry-title">Estatísticas</h1>
	</header><!-- .entry-header -->

	<div class="entry-content">
		<p><strong>Estatísticas IPv6 no Terra</strong>:</p>
<ul>
<ul>
<li><strong>10 Minutos</strong></li>
</ul>
</ul>
<div id="ipv6hourly_div" style="width: 600px; height: 250px;"></div>
<p>&nbsp;</p>
<ul>
<ul>
<li><strong>Diário</strong></li>
</ul>
</ul>
<div id="ipv6daily_div" style="width: 600px; height: 250px;"></div>
<p>&nbsp;</p>
<ul>
<ul>
<li><strong>Semana IPv6 (histórico)</strong></li>
</ul>
</ul>
<div id="ipv6week_div" style="width: 600px; height: 250px;"></div>
<p><script type="text/javascript" src="http://www.google.com/jsapi"></script><br />
<script type="text/javascript" src="../../../ipv6hourly.js"></script><br />
<script type="text/javascript" src="../../../ipv6daily.js"></script><br />
<script type="text/javascript">// <![CDATA[
	google.load('visualization', '1', {'packages': ['annotatedtimeline','corechart']});
	google.setOnLoadCallback(drawChartHourly);
	function drawChartHourly() {
		var table = new google.visualization.DataTable();
		table.addColumn('datetime', 'Date');
		table.addColumn('number', 'Native');
		table.addColumn('number', '6to4/Teredo');
		table.addColumn('number', 'Total IPv6');
		table.addColumn('number', 'IPv6 Broken');
		for (var i = 0; i < ipv6hourly.length; i++) {
			var d = ipv6hourly[i];
			var row = [
				new Date(d[0], d[1], d[2], d[3], d[4], d[5]),
				d[7], d[6]-d[7], d[6], d[8]
			];
			table.addRow(row);
		}
		var chart = new google.visualization.AnnotatedTimeLine(
			document.getElementById('ipv6hourly_div'));
		chart.draw(table, {allValuesSuffix: '%',
			displayExactValues: true,
			displayZoomButtons: false,
			scaleColumns: [0,0,0]});
	};
	google.setOnLoadCallback(drawChartDaily);
	function drawChartDaily() {
		var table = new google.visualization.DataTable();
		table.addColumn('date', 'Date');
		table.addColumn('number', 'Native');
		table.addColumn('number', '6to4/Teredo');
		table.addColumn('number', 'Total IPv6');
		table.addColumn('number', 'IPv6 Broken');
		for (var i = 0; i < ipv6daily.length; i++) {
			var d = ipv6daily[i];
			var row = [
				new Date(d[0], d[1], d[2]),
				d[4], d[3]-d[4], d[3], d[5]
			];
			table.addRow(row);
		}
		var chart = new google.visualization.AnnotatedTimeLine(
			document.getElementById('ipv6daily_div'));
		chart.draw(table, {allValuesSuffix: '%',
			displayExactValues: true,
			displayZoomButtons: false,
			scaleColumns: [0,0,0]});
	};
	google.setOnLoadCallback(drawChart);
		function drawChart() {
		var data = new google.visualization.DataTable();
		data.addColumn('string', 'IPv6 Origin');
		data.addColumn('number', 'Percent');
		data.addRows([
			['IPv6 vindos da Campus Party', 12.38],
			['Outros', 87.62]
		]);
		var options = {
			width: 600, height: 250,
			title: 'Semana IPv6'
		};
		var chart = new google.visualization.PieChart(document.getElementById('ipv6week_div'));
		chart.draw(data, options);
	}
// ]]&gt;</script></p>
