    }
    function Classificationweb()
    {
        var redirectURL = "";
        var title = document.getElementById("ctl00_m_g_3dbd8a5b_4e2c_4311_9523_806d2a098a2d_ctl00_txtTitle").value;
        alert(title);
        var clssif = document.getElementById("ctl00_m_g_3dbd8a5b_4e2c_4311_9523_806d2a098a2d_ctl00_ddlClassfication").value;
        alert (clssif);
        var url = document.getElementById("HidUrl").value;
        alert(url);
        if (title == "") redirectURL = clssif;
        else
        {
           if (clssif == "Arts Event" || clssif == "Video Game")
               redirectURL = clssif + "?mdaStrTitle=" + title;
           else redirectURL = clssif + "&sTitle=" + title);
        }
        window.open(redirectURL,"Classification","location=1,status=1,scrollbars=1,width=100,height=100");
    }
</script>

