    <script type="text/javascript">
        $(function() {
            function showRandomGlyph(){
                var maximum = 22;
                var minimum = 1;
                var randomnumber = Math.floor(Math.random() * (maximum - minimum + 1)) + minimum;
                $('#banner #glyph').attr('class','glyph'+randomnumber);
            }
            showRandomGlyph();

            function shiftAddenda(){
                $('#main-content .addendum').detach().appendTo('#addenda');
            }
            shiftAddenda();
            $('.datepicker').datepicker();

    		
        });
    </script>
    <script type="text/javascript" src="/apps/training-course-finder/js/training-course-finder.js"></script>
    
           <script type="text/javascript" src="/apps/training-home-sponsor-logos/js/training-home-sponsor-logos.js"></script> 
           <script type="text/javascript" src="/apps/training-tag-events/js/training-tag-events.js"></script> 
           <script type="text/javascript">
               function jumpToV6Courses(obj){
                   obj.preventDefault();
                   var dns_course_head = $('#homepage-course-list .course-list-group:first .course-list-group-head:first');
                   var ipv6_course_head = $('#homepage-course-list .course-list-group:first + .course-list-group .course-list-group-head:first');
                   var dns_course_body = $('#homepage-course-list .course-list-group:first .course-list-group-body:first');
                   var ipv6_course_body = $('#homepage-course-list .course-list-group:first + .course-list-group .course-list-group-body:first');
                   if(!ipv6_course_body.hasClass('in')){
                       ipv6_course_head.click();
                   }
                   if(dns_course_body.hasClass('in')){
                       dns_course_head.click();
                   }
                   document.getElementById('homepage-course-list').scrollIntoView();
               }
               $('.splash:first a:not(.bound)').addClass('bound').bind('click',jumpToV6Courses);


    // Collapse homepage course list (using bootstrap-collapse.js)

    // For each grouping of courses...
    $('#homepage-course-list .course-list-group').each(function(i){

        // Create a unique name for the group.
        var group_name = "course-list-group-" + $(this).find('.course-list-group-head').first().text().toLowerCase().replace(/\s/g, '_');

        // Get the group heading (the trigger for toggling).
        var trigger = $(this).find('.course-list-group-head').first();
        // Add bootstrap-collapse.js attrs to group heading.
        trigger.attr('data-toggle','collapse')
        trigger.attr('data-target','#' + group_name)

        // Get the group content (the target for toggling).
        var target  = $(this).find('.course-list-group-body').first();
        // Add unique name to the group content, and the toggle class.
        target.attr('id',group_name);
        target.addClass('collapse');
        // If this is the first group, show content by default.
        if(i==0){
            target.addClass('in');
        }
    });

    // Remove the event category labeling from name... we'll add a nice image in later.
    $('#homepage-events-list tr td a').each(function(){
         var text = $(this).text();
         var re =/^(eLearning: |Workshop: |Tutorial: )/g;
         var clean_text = text.replace(re,"");
         $(this).text(clean_text);
     });
     

            </script>
        		

    <script>
    // Add toggle arrows to collapsible content.
    $('.collapse').on('hide', function () {
        $(this).prev('*[data-toggle="collapse"]').find('.toggle-icon').addClass('toggle-state-off').removeClass('toggle-state-on');
    })

    $('.collapse').on('show', function () {
        $(this).prev('*[data-toggle="collapse"]').find('.toggle-icon').addClass('toggle-state-on').removeClass('toggle-state-off');
    })
    // Add toggle icons.
    $('#content *[data-toggle="collapse"]').append('<span class="toggle-icon toggle-state-off"></span>');
    $('#content *[data-toggle="collapse"] .toggle-icon').each(function(i){
        if($(this).parent().next('.collapse').hasClass('in')){
            $(this).addClass('toggle-state-on').removeClass('toggle-state-off');
        }
    });

    $('#course-page-list').dataTable( {
        "bPaginate": false,
        "bLengthChange": false,
        "bFilter": false,
        "bSort": true,
        "bInfo": true,
        "bAutoWidth": true 
    } );

    </script>

    		

