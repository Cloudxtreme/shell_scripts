$(document).ready(function () {
	$('#newsOne').rssfeed('http://broadband-forum.org/xml/broadband-forum_rss.xml',{}, function(e) {
		$(e).find('div.rssBody').vTicker();
	});

	$('#newsTwo').rssfeed('http://gamingstandards.com/rss/news.php',{}, function(e) {
		$(e).find('div.rssBody').vTicker();
	});

		
	$('#newsThree').rssfeed('http://nanog.org/rss.xml',{}, function(e) {
		$(e).find('div.rssBody').vTicker();
	});
	
			
	$('#newsFour').rssfeed('http://ntaforum.org/blog/?feed=rss2',{}, function(e) {
		$(e).find('div.rssBody').vTicker();
	});
	
	$('#newsFive').rssfeed('http://opennetworking.org/index.php?option=com_content&view=category&layout=blog&id=22&format=feed&type=rss',{}, function(e) {
		$(e).find('div.rssBody').vTicker();
	});

});
</script>
