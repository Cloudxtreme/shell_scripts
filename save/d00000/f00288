<script type="text/javascript"> $().ready(function() { /* Page specific Javascripts */ $('#sponsors-tabs span').click(function(){ $('#sponsors').attr('class',$(this).attr('class')); clearInterval(doit); }); var myTabs = new Array(); $('#sponsors-tabs span').each(function(){ myTabs.push($(this).attr('class')); }); function switchTabs(myTabs){ var currentTab = $('#sponsors').attr('class'); var newClass = ""; for (var i=0; i<myTabs.length; i++){ var myTab = myTabs[i]; if(myTab==currentTab){ if(i<(myTabs.length-1)){ newClass = myTabs[i+1]; } else{ newClass = myTabs[0]; } } } $('#sponsors').attr('class',newClass); } var doit = setInterval(function() { switchTabs(myTabs); }, 5000); }); 

</script>





