<script src="//www.google.com/jsapi"></script>
<script>
  google.load("feeds", "1");
  function loadDoodle() {
    var feed = new google.feeds.Feed("https://www.google.com/doodles/doodles.xml");
    feed.setNumEntries(1);
    feed.load(function(result) {
      if (!result.error) {
        var entry = result.feed.entries[0];
        var patt = /<img src=".*" alt=".*">/i
        var item_image = patt.exec(entry['content']);
        var item_title = entry['title'];
        var item_url = entry['link'];
        doodle_string = '<a href="' + item_url + '">' + item_image + '</a>' +
            '<p>' + item_title + '</p>';
        document.getElementById("about-latest-doodle").innerHTML = doodle_string;
      }
    });
  }
  function initialize() {
    loadDoodle();
  }
  google.setOnLoadCallback(initialize);
</script>

