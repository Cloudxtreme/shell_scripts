    <script type="text/javascript">
    $().ready(function() {
    /* Page specific Javascripts */
function loadDailyAgenda(here,andhere){


    // Get date and time from server
    $.get('/32/admin/server-date-time?foo=' + new Date().getTime(), function(data) {
  var servertime = data.split("-");
  var datestring = servertime[0] + "-" + servertime[1] + "-" + servertime[2];

    // Figure out URL to load based on day of week
    var dailyURL = "";
    var dailyURL2 = "";
    // Sat
    if ((datestring.indexOf('2011-7-27')!=-1)
        ||(location.hash.indexOf('sat')!=-1)){
           dailyURL = "/32/admin/homepage/saturday";
    }
    // Sun
    else if ((datestring.indexOf('2011-7-28')!=-1)
        ||(location.hash.indexOf('sun')!=-1)){
           dailyURL = "/32/admin/homepage/sunday";
    }
    // Mon
    else if ((datestring.indexOf('2011-7-29')!=-1)
        ||(location.hash.indexOf('mon')!=-1)){
           dailyURL = "/32/admin/homepage/monday";
    }
    // Tue
    else if ((datestring.indexOf('2011-7-30')!=-1)
        ||(location.hash.indexOf('tue')!=-1)){
           dailyURL = "/32/admin/homepage/tuesday";
    }
    // Wed
    else if ((datestring.indexOf('2011-7-31')!=-1)
        ||(location.hash.indexOf('wed')!=-1)){
           dailyURL = "/32/admin/homepage/wednesday";
    }
    // Thur am, Thurs pm, before conf, after conf
    else{
        var jsDate = new Date(servertime[0],servertime[1],servertime[2],servertime[3],servertime[4]);
        var beforeDate = new Date('2011','7','26','23','59');
        var afterDate = new Date('2011','8','1','18','30');
        // Before end of conf...
        if ((jsDate <= beforeDate)
        ||(location.hash.indexOf('before')!=-1)){
           dailyURL = "";
           dailyURL2 = "";
        }
        if ((datestring.indexOf('2011-8-1')!=-1)
        ||(location.hash.indexOf('thu')!=-1)){
           dailyURL = "/32/admin/homepage/thursday";
           dailyURL2 = "";
        }
        if ((jsDate >= afterDate)
        ||(location.hash.indexOf('after')!=-1)){
           dailyURL = "/32/admin/homepage/after";
           dailyURL2 = "/32/admin/homepage/after2";
        }

    }



if(dailyURL.length > 0){
    // Loading...
    $('#' + here).html('<img src="/__data/assets/image/0003/34626/wait30trans.gif" alt="Loading..."/>');
    $.get(dailyURL, function(data){ 
            $('#' + here).html(data);
            equalBoxHeight("feature1","feature2",18);
            if(dailyURL2.length > 0){
                $.get(dailyURL2, function(data){ 
                    $('#' + andhere).html(data);
            equalBoxHeight("feature1","feature2",18);
                });
            }
    });
}
else{
            equalBoxHeight("feature1","feature2",18);
}
   
});
}



//loadDailyAgenda('feature1','feature2');

$('#registerbutton').click(function(event) {
    event.preventDefault();
    window.open($(this).attr("href"));
});

function equalBoxHeight(id1,id2,em){
    var one = document.getElementById(id1);
    var two = document.getElementById(id2);
    var value = 0;
    var emvalue = em;
    if (one.offsetHeight > two.offsetHeight){value = one.offsetHeight;}
    else{value = two.offsetHeight;}
    
    value += emvalue;

    one.style.height = (value/emvalue) + "em";
    two.style.height = (value/emvalue) + "em";

}
equalBoxHeight("feature1","feature2",18);

        $('#ex2').jqm({ajax: '32/updates', trigger: 'a.ex2trigger',toTop: true});
    });
</script>

