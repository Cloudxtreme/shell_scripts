    var p = theImages.length;
    var preBuffer = new Array()
    for (i = 0; i < p; i++){
        preBuffer[i] = new Image()
        preBuffer[i].src = theImages[i][0]
    }
    var whichImage = Math.round(Math.random()*(p-1));

    document.getElementById(output).innerHTML = '<a href="'+theImages[whichImage][1]+'" class="image"><img src="'+theImages[whichImage][0]+'"></a>';
}
randomSponsor("randomsponsor"); 

</script>

