<script type="text/javascript">
jQuery(document).ready(function(){
	jQuery('#companyRow').hide();
	if(jQuery('#country').val() == 'US') {
		jQuery('#state1').show();
		jQuery('#state').hide();
	}
	else{
		jQuery('#state1').hide();
		jQuery('#state').show();
	}
	jQuery('#acctype').change(function() {
		if(jQuery('#acctype').val() != '')
		{
			if(jQuery('#acctype').val() == 'company') {
				jQuery('#companyRow').show()
			} else {
				jQuery('#companyRow').hide();
			}
			jQuery("#acctype").css({"background-color": "lightblue"});
			jQuery("#error_acctype").css({"color":"blue"});
			jQuery("#error_acctype").html("&#10003;");
		}
		else
		{
			jQuery("#acctype").css({"background-color": "#f86d6d"});
			jQuery("#error_acctype").css({"color":"red"});
			jQuery("#error_acctype").html("invalid selection");
		}
	});

	jQuery('#state1').change(function() {
		if(jQuery('#state1').val() != '')
		{
			jQuery("#state1").css({"background-color": "lightblue"});
			jQuery("#error_state1").css({"color":"blue"});
			jQuery("#error_state1").html("&#10003;");
		}
		else
		{
			jQuery("#state1").css({"background-color": "#f86d6d"});
			jQuery("#error_state1").css({"color":"red"});
			jQuery("#error_state1").html("invalid selection");
		}
	});

	jQuery('#country').change(function() {
		jQuery("#error_state1").html("");
		jQuery("#error_state").html("");
		jQuery("#state1").css({"background-color": "#fff"});
		jQuery("#state").css({"background-color": "#fff"});
		if(jQuery('#country').val() != '')
		{
			if(jQuery('#country').val() == 'US') {
				jQuery('#state1').show();
				jQuery('#state').hide();
			}
			else{
				jQuery('#state1').hide();
				jQuery('#state').show();
			}
			jQuery("#country").css({"background-color": "lightblue"});
			jQuery("#error_country").css({"color":"blue"});
			jQuery("#error_country").html("&#10003;");
		}
		else
		{
			jQuery("#country").css({"background-color": "#f86d6d"});
			jQuery("#error_country").css({"color":"red"});
			jQuery("#error_country").html("invalid selection");
		}
	});

	jQuery('#register').submit(function(){
		if(jQuery('#username').val() == '') { alert("Account name field cannot be empty!"); jQuery('#username').focus();  return false; }
		else if(jQuery('#email').val() == '') { alert("Email field cannot be empty!"); jQuery('#email').focus();  return false; }
		else if(jQuery('#fname').val() == '') { alert("First name field cannot be empty!"); jQuery('#fname').focus();  return false; }
		else if(jQuery('#lname').val() == '') { alert("Last name field cannot be empty!"); jQuery('#lname').focus();  return false; }
		else if(jQuery('#acctype').val() == "company") {
			if(jQuery('#company').val() == '') { alert("Company field cannot be empty!"); jQuery('#lname').focus();  return false; }
		}
		else if(jQuery('#country').val() == '') { alert("Country field cannot be empty!"); jQuery('#country').focus();  return false; }
		else if(jQuery('#country').val() != 'US') { 
			if(jQuery('#state').val() == '') { alert("State field cannot be empty!"); jQuery('#state').focus();  return false; } 
		}
		else if(jQuery('#street').val() == '') { alert("Street field cannot be empty!"); jQuery('#street').focus();  return false; }
		else if(jQuery('#city').val() == '') { alert("City field cannot be empty!"); jQuery('#city').focus();  return false; }
		else if(jQuery('#zip').val() == '') { alert("Zip code field cannot be empty!"); jQuery('#zip').focus();  return false; }
		else if(jQuery('#phone').val() == '') { alert("Phone field cannot be empty!"); jQuery('#phone').focus();  return false; }
		else return true;
	});
});
</script>
