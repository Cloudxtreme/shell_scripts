        <TR VALIGN="TOP" ALIGN="LEFT">
            <TD COLSPAN="3" WIDTH="43">
                <P><A HREF="../../Florida_Reisefuehrer/florida_reisefuehrer.html"><FONT SIZE="-1"><B>Zurück</B></FONT></A></P>
            </TD>
            <TD COLSPAN="8"></TD>
