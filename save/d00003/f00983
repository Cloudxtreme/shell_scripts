$(document).ready(function() {
  $('#slide-box-main').cycle({ 
    fx: 'fade', 
    speed: 'slow', 
    timeout: 10000, 
    pager: '#slide-box-main .page-slide',

  });
  $('#slide-box-main .resumeButton').click(function() { 
    $('#slide-box-main').cycle('resume');
    $("#slide-box-main .resumeButton").addClass('activeSlide');
    $("#slide-box-main .pauseButton").removeClass('activeSlide');
  });

  $('#slide-box-main .pauseButton').click(function() { 
    $('#slide-box-main').cycle('pause');
    $("#slide-box-main .pauseButton").addClass('activeSlide');
    $("#slide-box-main .resumeButton").removeClass('activeSlide');
  });
        });
</script>


