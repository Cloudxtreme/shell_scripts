  $('#bc-main .resumeButton').click(function() { 
    $('#bc-main').cycle('resume');
    $("#bc-main .resumeButton").addClass('activeSlide');
    $("#bc-main .pauseButton").removeClass('activeSlide');
  });

  $('#bc-main .pauseButton').click(function() { 
    $('#bc-main').cycle('pause');
    $("#bc-main .pauseButton").addClass('activeSlide');
    $("#bc-main .resumeButton").removeClass('activeSlide');
  });
});
</script>

