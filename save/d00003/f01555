<script language="JavaScript" >
function getCountrys(area_code) {
//	alert("getCountrys");
//	alert("area_code:" +area_code);
    var tmpAreaCode="";
    if(area_code!=''){
    	if(area_code=='1'){
    		$("#titleCony").text('Europe');
    		document.getElementById("areaCodeMobile").value="1";
    		tmpAreaCode = "1";
    	}else if(area_code=='2'){
    		$("#titleCony").text('Asia');
    		document.getElementById("areaCodeMobile").value="2";
    		tmpAreaCode="2";
    	}else if(area_code=='3'){
    		$("#titleCony").text('Africa');
    		document.getElementById("areaCodeMobile").value="3";
    		tmpAreaCode="3";
    	}else if(area_code=='4'){
    		$("#titleCony").text('Oceania');
    		document.getElementById("areaCodeMobile").value="4";
    		tmpAreaCode="4";
    	}else if(area_code=='5'){
    		$("#titleCony").text('America');
    		document.getElementById("areaCodeMobile").value="5";
    		tmpAreaCode="5";
    	}
    	
    	$("#RR1").empty();
    	$("#RR2").empty();
    	$("#RP1").empty();
    	$.ajax({
    		url: '/portal/Area_items',
    		data: {method: 'queryAreaItems',area_code :area_code},
    		dataType: "json",

    		error: function(xhr) {alert("錯誤!"); },
    		success: function(response) {
    			ajaxData(response);
    			btnClick(document.getElementById('testconn'));
    		}
    		
    	});
	}
}

function getCountrysMobile() {
	var area_code = document.getElementById("area_code").value; 
	// alert("area_code:" +area_code);
    if(area_code!=''){
    	if(area_code=='1'){
    		$("#titleCony").text('Europe');
    	}else if(area_code=='2'){
    		$("#titleCony").text('Asia');
    	}else if(area_code=='3'){
    		$("#titleCony").text('Africa');
    	}else if(area_code=='4'){
    		$("#titleCony").text('Oceania');
    	}else if(area_code=='5'){
    		$("#titleCony").text('America');
    	}
    	else if(area_code=='air'){
    		window.location.href="/portal/AirRate2?areaCodeMobile=air_red";
    		return;
    	}
    	else if(area_code=='mar'){
    		window.location.href="/portal/CruiseRate2?areaCodeMobile=mar_red";
    		return;
    		
    	}
    	
    	$("#RR1").empty();
    	$("#RR2").empty();
    	$("#RP1").empty();
    	$.ajax({
    		url: '/portal/Area_items',
    		data: {method: 'queryAreaItems',area_code :area_code},
    		dataType: "json",

    	//	error: function(xhr) {alert("錯誤!"); },
    	    error: function(xhr) { },
    		success: function(response) {
    			ajaxData(response);
    		}
    		
    	});
	}
}


function getCountrysMobileLoad() {
	var area_code = document.getElementById("area_code").value; 
	// alert("area_code:" +area_code);
    if(area_code!=''){
    	if(area_code=='1'){
    		$("#titleCony").text('Europe');
    	}else if(area_code=='2'){
    		$("#titleCony").text('Asia');
    	}else if(area_code=='3'){
    		$("#titleCony").text('Africa');
    	}else if(area_code=='4'){
    		$("#titleCony").text('Oceania');
    	}else if(area_code=='5'){
    		$("#titleCony").text('America');
    	}
    	else if(area_code='air'){
    	//	window.location.href="/portal/AirRate2?areaCodeMobile=air_red";
    		return;
    	}
    	else if(area_code='mar'){
    	//	window.location.href="/portal/CruiseRate2?areaCodeMobile=mar_red";
    		return;
    		
    	}
    	else{
    		return;
    	}
    	
    	$("#RR1").empty();
    	$("#RR2").empty();
    	$("#RP1").empty();
    	$.ajax({
    		url: '/portal/Area_items',
    		data: {method: 'queryAreaItems',area_code :area_code},
    		dataType: "json",

    		error: function(xhr) {alert("錯誤!"); },
    		success: function(response) {
    			ajaxData(response);
    		}
    		
    	});
	}
}




function ajaxData(response){
	var a1 = 0;
	var len = (response.length )/2;
	 $.each(response, function(index, obj) {
		 a1 = index+1;
		 if(a1 <=len){
			 if(a1<10){
				// $('#RR1').append($( "<li><span class='no'>0"+a1+"</span>   <a onclick=getCon('"+obj.c_country+ "'); ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
				 $('#RR1').append($( "<li><span class='no'>0"+a1+"</span>   <a href=/"javascript:getCon('"+obj.c_country+ "');\" ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
			 }else{
				// $('#RR1').append($( "<li><span class='no'>"+a1+"</span>   <a onclick=getCon('"+obj.c_country+ "'); ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
				 $('#RR1').append($( "<li><span class='no'>"+a1+"</span>    <a href=/"javascript:getCon('"+obj.c_country+ "');\" ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
			 }
		 }else{
			 if(a1<10){
			 //	$('#RR2').append($( "<li><span class='no'>0"+a1+"</span>   <a onclick=getCon('"+obj.c_country+ "'); ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
				 $('#RR2').append($( "<li><span class='no'>0"+a1+"</span>   <a href=/"javascript:getCon('"+obj.c_country+ "');\" ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
			 }else{
			//	$('#RR2').append($( "<li><span class='no'>"+a1+"</span>   <a onclick=getCon('"+obj.c_country+ "'); ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" )); 
				$('#RR2').append($( "<li><span class='no'>"+a1+"</span>     <a href=/"javascript:getCon('"+obj.c_country+ "');\" ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" )); 
			 }
		 }	
		 if(a1<10){
			// $('#RP1').append($( "<li><span class='no'>0"+a1+"</span>   <a onclick=getCon('"+obj.c_country+ "'); ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
			 $('#RP1').append($( "<li><span class='no'>0"+a1+"</span>       <a href=/"javascript:getCon('"+obj.c_country+ "');\" ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
		 }else{
			// $('#RP1').append($( "<li><span class='no'>"+a1+"</span>   <a onclick=getCon('"+obj.c_country+ "'); ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
			 $('#RP1').append($( "<li><span class='no'>"+a1+"</span>        <a href=/"javascript:getCon('"+obj.c_country+ "');\" ><span class='en'>" + obj.e_country+"</span><span class='tw'>"+ obj.c_country +"</span></a>" ));
		 }
	 });
	 //$('#RR1').append($( yyy));
//	btnClick(document.getElementById('testconn'));

	//var oDiv = document.getElementById('testconn');//.attachEvent("onclick", fn1);
	 //document.getElementById('testconn').fireEvent("onclick");//.click();
}	

function getCon(code){
	$.colorbox.close();
	if(code !=''){
		document.getElementById('country').value = encodeURIComponent(code);
		doSubmit();
		getCountrysMobile();
	}
}
function doSubmit() {
	$("#Rate").submit();
	//frm.submit();

}

