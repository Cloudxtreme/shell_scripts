<script type="text/javascript"  >
$(document).ready(function(){
    $('#mobileTabMenu').change(function(){
        //alert($(this).children('option:selected').val());
        window.location.href = $(this).children('option:selected').val();
    });
    
    var objbutton = $(".read-more a[href='#readmore']");
	  objbutton.click();
	  
	  var bdd=$(".breadcrumb").text().replace(/[\r\n]/g,"");
    var sarr=bdd.split('>');
    var i=2;
    $("menu.aside-menu li a").each(function(){
      var s=($(this).text()).replace(/(^[\s]*)|([\s]*$)/g, "");
      if (i<sarr.length && i==2) {
        var q=sarr[i].replace(/(^[\s]*)|([\s]*$)/g, "");
        if (s==q) { 
          $(this).addClass("selected");
          i++;
        }
      }
      else if (i<=sarr.length && i>2) {
        var q=sarr[i].replace(/(^[\s]*)|([\s]*$)/g, "");
        if (s==q) { 
          //open level 
          var par=$(this).parents('menu .aside-menu-sub');
          if($(par).attr('class') == 'aside-menu-sub'){
            $(par).attr('style','display: block;');
          }
          
          var a= $(this).html();
        	var b= "<span class=\"selected\">"+a+"</span>";
          $(this).html(b);
          i++;
        }
      }
      if (i>=sarr.length) { return false; }
    });
	
    $("#nonepromo").addClass("top-cap-nosale");
    
})
</script>
<script type="text/javascript" >
$(function () {
        $("#txtKeyword").keypress(function(event) {
            if (event.keyCode == '13') {
            $('#btnSearch').trigger('click');
            event.preventDefault();
            }
        });

        $("#btnSearch").click(function() {
            if ($("#txtKeyword").val() == '') {
                alert('請輸入搜尋關鍵字！');
                return false;
            }
            
            $.ajax({
              url: 'http://redir.hinet.net/r/14238',
              data: {np: 'http://www.cht.com.tw'},
              dataType: "json",
              
              error: function(xhr) {},
              success: function(response) {}
            });
            location.href="http://search.cht.com.tw/Search?searchTemplate=" + 16 + "&Keyword=" + encodeURIComponent($("#txtKeyword").val());
        });
        
        $("a[href='#ft-lightbox-4']").colorbox({inline:true, width:680});
});
</script>
