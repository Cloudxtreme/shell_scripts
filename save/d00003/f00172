		 data-nifty-socialplugins-mixi-apikey="d743f911bb6f2e1cf0ea467a0991c3aed475db54"
		 data-nifty-socialplugins-url="http://woman-money.nifty.com/intelligence/senior/detail/130214000505_1.htm"
