<script type="text/javascript">
$(document).ready(function() {
if ($("#search-hendJDYywoNLOTlnstKhxKxSp-q").val() != "") {
$("#search-hendJDYywoNLOTlnstKhxKxSp-clearButton").addClass("visible");
$("#search-hendJDYywoNLOTlnstKhxKxSp-searchFieldset").addClass("hasText");
}
$("#search-hendJDYywoNLOTlnstKhxKxSp-clearButton").click(function(e){
$("#search-hendJDYywoNLOTlnstKhxKxSp-q").val("");
$(this).removeClass("visible");
$('#search-hendJDYywoNLOTlnstKhxKxSp-searchFieldset').removeClass('hasText');
e.preventDefault();
$('#search-hendJDYywoNLOTlnstKhxKxSp-q').focus();
});
$("#search-hendJDYywoNLOTlnstKhxKxSp-q").on("keyup", function(e) {
if ($("#search-hendJDYywoNLOTlnstKhxKxSp-q").val() != "") {
$("#search-hendJDYywoNLOTlnstKhxKxSp-clearButton").addClass("visible");
$("#search-hendJDYywoNLOTlnstKhxKxSp-searchFieldset").addClass("hasText");
} else {
$("#search-hendJDYywoNLOTlnstKhxKxSp-clearButton").removeClass("visible");
}
});
$("#search-hendJDYywoNLOTlnstKhxKxSp-filterButton").click(function(e){
$(".filterParameter").remove();
$("#search-hendJDYywoNLOTlnstKhxKxSp-searchFieldset").removeClass("filterd");
$(this).hide();
});
});
</script>   
