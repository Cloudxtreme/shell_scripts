<script type="text/javascript"> var previousMenuItemShowingKey = 'mega-menu-block-1-260064MenuItemShowing'; // The fontFamily attribute on #propertiesMegaMenu is set by the css to indicate
// that the mega-menus menus should not be shown.
var viewState = $('#propertiesMegaMenu').css('fontFamily');
if (viewState == "property-view-state_maximized") {
$('.bodyWrapper').click(function(event) {
var previousMenuItemShowing = jQuery.data(document.body, previousMenuItemShowingKey);
if (previousMenuItemShowing) {
$(previousMenuItemShowing).hide();
}
});
$('#menu-group-1-260064-354068286_header').click(function(event) {
var nextMenuItemContentToShow = '#menu-group-1-260064-354068286_menuItemContent';
if ($(nextMenuItemContentToShow).is(":hidden")) {
var previousMenuItemShowing = jQuery.data(document.body,
previousMenuItemShowingKey);
event.preventDefault();
if (previousMenuItemShowing) {
$(previousMenuItemShowing).hide();
}
jQuery.data(document.body, previousMenuItemShowingKey,
nextMenuItemContentToShow);
$(nextMenuItemContentToShow).show();
}
event.stopPropagation();
});
