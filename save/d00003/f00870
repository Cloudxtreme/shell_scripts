    </div><script src="//www.google.com/jsapi">
</script><script>
google.load('feeds', '1');
      function getFeed(url, containerEl) {
        var feed = new google.feeds.Feed(url);
        feed.setNumEntries(5);
        feed.includeHistoricalEntries();
        feed.load(function(result) {
          if (!result.error) {
            var listEl = document.createElement('ul');
            listEl.className = 'press-releases';
            for (var i = 0; i < result.feed.entries.length; i++) {
              var entry = result.feed.entries[i];
              var entryEl = document.createElement('li');
              var linkEl = document.createElement('a');
              linkEl.href = entry.link.replace(/googlechinablog\.blogspot\.com/gi,
                  'www.google.com.hk/ggblog/googlechinablog');
              linkEl.appendChild(document.createTextNode(entry.title));
              var dateEl = document.createElement('span');
              dateEl.className = 'press-date';
              var dateString = new Date(entry.publishedDate).toLocaleDateString();
              dateEl.appendChild(document.createTextNode(dateString));
              entryEl.appendChild(linkEl);
              entryEl.appendChild(document.createElement('br'));
              entryEl.appendChild(dateEl);
              if (entry.contentSnippet) {
                entryEl.appendChild(document.createElement('br'));
                entryEl.appendChild(document.createTextNode(entry.contentSnippet));
              }
              listEl.appendChild(entryEl);
            }
            containerEl.innerHTML = '';
            containerEl.appendChild(listEl);
          }
        });
      };
      function initFeeds() {
        getFeed(document.getElementById('press-blog-feed').href,
            document.getElementById('press-blog-posts'));
      }
      google.setOnLoadCallback(initFeeds);
    </script><script src="//www.google.com/js/maia.js">
</script>
