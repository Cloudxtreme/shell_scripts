<script type="text/javascript">
$(document).ready(function() {
if ($("#search-NdgrSpWXsYcdvEjepFFbeHSyW-q").val() != "") {
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-clearButton").addClass("visible");
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-searchFieldset").addClass("hasText");
}
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-clearButton").click(function(e){
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-q").val("");
$(this).removeClass("visible");
$('#search-NdgrSpWXsYcdvEjepFFbeHSyW-searchFieldset').removeClass('hasText');
e.preventDefault();
$('#search-NdgrSpWXsYcdvEjepFFbeHSyW-q').focus();
});
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-q").on("keyup", function(e) {
if ($("#search-NdgrSpWXsYcdvEjepFFbeHSyW-q").val() != "") {
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-clearButton").addClass("visible");
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-searchFieldset").addClass("hasText");
} else {
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-clearButton").removeClass("visible");
}
});
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-filterButton").click(function(e){
$(".filterParameter").remove();
$("#search-NdgrSpWXsYcdvEjepFFbeHSyW-searchFieldset").removeClass("filterd");
$(this).hide();
});
});
</script>   
