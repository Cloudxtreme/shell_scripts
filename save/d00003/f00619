<script type="text/javascript">/* version 1.5 */

$(document).ready(function() {

	  // Mega menu
	  $('.block.megaMenu a').on('mousedown', function(event) {
		_gas.push(['_trackEvent', 'Mega menu', quoteReplace($(event.target).html()), event.target.getAttribute('href')]);
		console.log("Mega menu: " + "'" + quoteReplace($(event.target).html()) + "'");
	  });

	  // Feed back
	  $('.contentResponsible a').on('mousedown', function(event) {
	    _gas.push(['_trackEvent', 'Feedback', getDocumentTitle()]);
	    console.log("Feed back: " + "'" + getDocumentTitle() + "'");
	  });

	  // Curriculum
	  $('.callToAction.completeCurriculum').on('mousedown', function(event) {
	    _gas.push(['_trackEvent', 'Complete Curriculum', getDocumentTitle(), event.target.getAttribute('href')]);
	    console.log("Curricuculum: " + "'" + getDocumentTitle() + "' (" + event.target.getAttribute('href') + ")");
	  });

	  // Banners and Teasers
	  $('.block.teaser').on('mousedown', function(event) {  
		var $this = $(this);
		var hRef = $this.find('h2 a').attr('href');
		var target = $(event.target);
		
		if (hRef && typeof(target.attr('href')) != 'undefined' && hRef != target.attr('href')) {
		  hRef = target.attr('href');
		} else if (!hRef && typeof(target.attr('href')) != 'undefined') {
		  hRef = target.attr('href');
		}
		
		if ($this.hasClass('overlay')) { // Banner
		  if (target.is("a, a img")) {
			_gas.push(['_trackEvent', 'Banner', findTeaserH2Text($this), hRef]);
		    console.log("Banner:  " + findTeaserH2Text($this) + ' (' + hRef + ')');
		  }
		} else { // Teaser
		  if (target.is('a:not([[href*="mailto"]), a img')) {
			_gas.push(['_trackEvent', 'Teaser', findTeaserH2Text($this), hRef]);
			console.log("Teaser:  " + findTeaserH2Text($this) + ' (' + hRef + ')');
		  }
		}
	  });
	  	  
	  // Dropdowns
	  $('.block.list.dropdown form select').change(function() {
		  _gas.push(['_trackEvent', 'Dropdown', quoteReplace($(this).find('option[value="' + $(this).val() + '"]').text()), $(this).val()]);
	  });
	  
	  // Form error
	  if($('.article.standard.contact .contactForm .error h2').length != 0) {
		_gas.push(['_trackPageview','/virtual/form - error ' + document.location.pathname + document.location.search]);
		console.log("Form error at " + document.location.pathname + document.location.search);
	  }
	  
	  // Form sent
	  if($('.article.standard.contact .formSent').length != 0) {
		_gas.push(['_trackPageview', '/virtual/formular - sent' + document.location.pathname + document.location.search]);
		console.log("Form sent from " + document.location.pathname + document.location.search);
	  }

          // Special form tracking
	  $('form .buttons .dataManipulation input, form#material input.submitForm').on('mousedown', function(event) {
		_gas.push(['_trackEvent', 'Form', 'Sent', location.pathname]);
		console.log("Form sent from " + location.pathname);
	  });
	  
	  // LinkedIn
	  $('.shareLarge .linkedin').on('mousedown', function(event) {
	    _gas.push(['_trackSocial', 'LinkedIn', 'share', document.location.href]);
	    console.log("Linked in event tracked");
	  });
	  
	  // Twitter
	  function trackTwitter(event) {
	    if (event) {
		  var targetUrl;
		  if (event.target && event.target.nodeName == 'IFRAME') {
		    targetUrl = extractParamFromUri(event.target.src, 'url');
		  }
		  _gas.push(['_trackSocial', 'twitter', 'tweet', targetUrl]);
		  console.log("Twitter event tracked");
	    }
      }

	if (typeof(twttr) != 'undefined') {
	    //Wrap event bindings - Wait for async js to load
        twttr.ready(function (twttr) {
          twttr.events.bind('tweet', trackTwitter);
        });
	  }
	  
       function extractParamFromUri(uri, paramName) {
		if (!uri) {
		  return;
		}
		var regex = new RegExp('[\\?&#]' + paramName + '=([^&#]*)');
	    var params = regex.exec(uri);
		if (params != null) {
		  return unescape(params[1]);
		}
		return;
          }

	  // Facebook
	  if (typeof(FB) != 'undefined') {
	    if (FB && FB.Event && FB.Event.subscribe) {
          FB.Event.subscribe('edge.create', function(targetUrl) {
            _gas.push(['_trackSocial', 'facebook', 'like', targetUrl]);
            console.log("FB like");
          });
	  
          FB.Event.subscribe('edge.remove', function(targetUrl) {
            _gas.push(['_trackSocial', 'facebook', 'unlike', targetUrl]);
            console.log("FB unlike");
          });
	  
          FB.Event.subscribe('message.send', function(targetUrl) {
            _gas.push(['_trackSocial', 'facebook', 'send', targetUrl]);
            console.log("FB send");
          });
      
          FB.Event.subscribe('comment.create', function(targetUrl) {
            _gas.push(['_trackSocial', 'facebook', 'comment', targetUrl]);
            console.log("FB comment");
	      });
	    }
	  }
	  
	  // Page vote function
	  $('#vote').click(function(event) {
		var docTitle = quoteReplace(document.title);
		var e = event.target;
		
		if(e && $(e).hasClass('icon-thumbs-up')) {
		  _gas.push(['_trackEvent', 'Voting', 'Yes', docTitle, 1]);
		  console.log('Voting "YES" for page: ' + docTitle);
		} else if(e && $(e).hasClass('icon-thumbs-down')) {
                  // Using gaq due to bug in gas
 		  _gaq.push(['_trackEvent', 'Voting', 'No', docTitle, -1]);
		  console.log('Voting "NO" for page: ' + docTitle);
		}
	  });
	  
	  // Error page
	  if ($('.errorPage').length != 0) {
        var errCode = getErrorCode($(this).attr('class'));
        _gas.push(['_trackPageview','/' + errCode + '.jsp?page=' + document.location.pathname + document.location.search + '&from=' + document.referrer]);
		console.log("Page error " + '_trackPageview','/' + errCode + '.jsp?page=' + document.location.pathname + document.location.search + '&from=' + document.referrer);
	  }
	  
	  /**
	   * Get the error code if available.
	   */
	  function getErrorCode(classes) {
		  var regExpErrCode = /.*error_code_(\w+).*/;
		  
		  if (classes && classes.match(regExpErrCode)) {
            return classes.replace(regExpErrCode, $1);
		  }
		  return "";
	  }
	  
	  /**
	   * Get the h2 text from the teaser.
	   */
	  function findTeaserH2Text(e) {
		  if (e.find('h2 a').html()) {
			  return quoteReplace(e.find('h2 a').html());
		  } else if (e.find('h2').html()) {
			  return quoteReplace(e.find('h2').html());
		  } else {
			  return "";
		  }
	  }
	  
	  /**
	   * Replace quotes from the given string.
	   */
	  function quoteReplace(s) {
	      if (s) {
			  s = s.replace(/\'/g,"\\'");
		  }
	      return s;
	  }
	  
	  /**
	   * Get the document title.
	   */
	  function getDocumentTitle() {
		  return quoteReplace(document.title.replace('KTH | ',''));
	  }
	  
});</script>  
