<script type="text/javascript">
	/*<![CDATA[*/
<!--
	function openPic(url,winName,winParams)	{	//
		var theWindow = window.open(url,winName,winParams);
		if (theWindow)	{theWindow.focus();}
	}

			var rotator = new Array();
			var play = true;

			function theRotator(uid, delay, dynamic) {
				rotator[uid] = new Object();
				rotator[uid]['dynamic'] = dynamic;
				rotator[uid]['delay'] = delay;
				if (dynamic) {
					rotator[uid]['current'] = Math.floor(Math.random() * $('div#rnd' + uid + ' div.image').length );
				} else {
					rotator[uid]['current'] = 0;
				}
				$('div#rnd' + uid + ' div.caption').hide();
				$('div#rnd' + uid + ' div.image').hide();
				$('#rnd'+uid+'_'+rotator[uid]['current']).addClass('show')
				.show();
				$('#rnd'+uid+'_'+rotator[uid]['current']+'c').addClass('show')
				.show();
				rotator[uid]['handler'] = setInterval('rotate('+uid+')',delay);
			}
			
			function rotate(uid) {
				if (rotator[uid]['dynamic']) {
					var next = Math.floor(Math.random() * $('div#rnd' + uid + ' div.image').length );
					if (rotator[uid]['current'] == next) next++;
				} else {
					next = rotator[uid]['current'] + 1;
				}
				if (!$('#rnd'+uid+'_'+next).length) next = 0;
				$('#rnd'+uid+'_'+rotator[uid]['current']+'c').fadeOut()
				.removeClass('show');
				$('#rnd'+uid+'_'+rotator[uid]['current']).fadeOut()
				.removeClass('show');
				$('#rnd'+uid+'_'+next).fadeIn()
				.addClass('show');
				$('#rnd'+uid+'_'+next+'c').fadeIn()
				.addClass('show');
				rotator[uid]['current'] = next;
			};

			function rotateTo(uid, next) {
				if (rotator[uid]['current'] == next) return false;
				clearInterval(rotator[uid]['handler']);
				$('#rnd'+uid+'_'+rotator[uid]['current']+'c').fadeOut()
				.removeClass('show');
				$('#rnd'+uid+'_'+rotator[uid]['current']).fadeOut()
				.removeClass('show');
				$('#rnd'+uid+'_'+next).fadeIn()
				.addClass('show');
				$('#rnd'+uid+'_'+next+'c').fadeIn()
				.addClass('show');
				rotator[uid]['current'] = next;
				rotator[uid]['handler'] = setInterval('rotate('+uid+')',rotator[uid]['delay']);
				return false;
			};
			
			function switchAnimation(uid) {
				if (play) {
					play = false;
					$('.stop').attr('src', 'typo3conf/ext/univie_bannerrotation//image/play.png');
					$('.stop').attr('title', 'Bildrotation starten');
					clearInterval(rotator[uid]['handler']);
				} else {
					play = true;
					$('.stop').attr('src', 'typo3conf/ext/univie_bannerrotation//image/stop.png');
					rotator[uid]['handler'] = setInterval('rotate('+uid+')',rotator[uid]['delay']);
					$('.stop').attr('title', 'Bildrotation anhalten');
				}
			}

		

// -->
	/*]]>*/
</script>

