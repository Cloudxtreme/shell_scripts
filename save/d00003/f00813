// GA Baseline Tagging
var GA_num = 2;

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-9435527-1']);
  _gaq.push(['_setDomainName', 'singtel.com']);
  _gaq.push(['_setAllowLinker', true]);

  _gaq.push(['acct2._setAccount', 'UA-9435527-4']);
  _gaq.push(['acct2._setDomainName', 'singtel.com']);
  _gaq.push(['acct2._setAllowLinker', true]);

  _gaq.push(['_trackPageview']);
  _gaq.push(['acct2._trackPageview']);

  function gaTrack(url){
    _gaq.push(['_trackPageview',url]);
    if (GA_num != 1) _gaq.push(['acct2._trackPageview',url]);
  }

  function gaEvent(cat, action, label){
    _gaq.push(['_trackEvent', cat, action, label]);
    if (GA_num != 1) _gaq.push(['acct2._trackEvent', cat, action, label]);
  }

  function trackPageview(url){
    _gaq.push(['_trackPageview',url]);
    if (GA_num != 1) _gaq.push(['acct2._trackPageview',url]);
  }

  function trackEvent(cat, action, label){
    _gaq.push(['_trackEvent', cat, action, label]);
    if (GA_num != 1) _gaq.push(['acct2._trackEvent', cat, action, label]);
  }

  var singtelGA = {
      trackLinkClick: function(component){
			singtelGA.trackLinkClickWithLabel(component,"Links");
      },
      trackLinkClickWithLabel: function(component,category){
			title = jQuery(component).html() + " | ";
			if(title.indexOf('<') != -1)
				title = "";
			href =  title + jQuery(component).attr("href") ;
			if(href.indexOf('http') == -1 || href.indexOf('singtel.com') !== -1)
				singtelGA.trackEvent(category,"Click", href);
			else
				singtelGA.trackEvent(category,"Exit", href);
      },
      trackEvent: function(category, action, opt_label){
      		_gaq.push(['_trackEvent', category, action, opt_label]);
			if (GA_num != 1) _gaq.push(['acct2._trackEvent', category, action, opt_label]);
      },
      trackPageview: function(url){
      		_gaq.push(['_trackPageview', url]);
			if (GA_num != 1) _gaq.push(['acct2._trackPageview', url]);
      }
  };

