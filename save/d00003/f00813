$(window).unload(function(){
if(location.pathname.split("/")[1] == "sme" || location.pathname.split("/")[1] == "large-enterprise" || location.pathname.split("/")[1] == "business") {
        SetCookie('business','yes', exp);
        var biztimer = new Date();
        SetCookie('biztimer',Date.parse(biztimer), exp)
}
else{
        SetCookie('business','no', exp);
        SetCookie('biztimer', 0, exp)
}
});
</script>

