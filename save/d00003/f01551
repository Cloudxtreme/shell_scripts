		<div id="header-counter">
			<a href="http://dane.podnikatel.cz/dan-z-prijmu/?utm_source=Podnikatel&utm_medium=lista&utm_campaign=Dane"><span class="only">Už jen</span><span id="header-days"> dní</span><span id="header-time"></span>
				zbývá na <strong>podání přiznání k dani z příjmů</strong>. <span class="linkMark">Poradíme vám, jak na to!</span></a>
		</div>
		<script type="text/javascript">
			(function() {
				function formatTime(days) {
					var hours = (days  - Math.floor(days)) * 24.0;
					var minutes = (hours - Math.floor(hours)) * 60.0;
					var seconds = (minutes - Math.floor(minutes)) * 60.0;
					return (hours >= 1 ? Math.floor(hours) + " h " : "")
						+ (minutes >= 1 ? Math.floor(minutes) + " min " : "")
						+ Math.floor(seconds) + " s";
				}

				var endDate = new Date(2013, 2, 31, 23, 59, 59, 0);
				var el = document.getElementById("header-counter");
				var daysEl = document.getElementById("header-days");
				var timeEl = document.getElementById("header-time");
				if (el && daysEl && timeEl) {
					var interval = setInterval(function() {
						var now = new Date();
						var timeDiff = (endDate.valueOf() - now.valueOf()) / 86400000.0;

						if (timeDiff >= 1) {
							daysEl.innerHTML = Math.floor(timeDiff) + " d ";
						} else {
							daysEl.innerHTML = "";
						}
						timeEl.innerHTML = formatTime(timeDiff);

						if (timeDiff < 0) {
							clearInterval(interval);
							el.style.display = "none";
						} else {
							el.style.display = "block";
						}
					}, 1000);
				}
			})();
		</script>
		<!--<![endif]-->
