	<script type="text/javascript">
		var map = null;
		var geocoder = new google.maps.Geocoder();
		function codeAddress(address, zoom) {
			geocoder.geocode({ 'address': address}, function(results, status) {
				if(status == google.maps.GeocoderStatus.OK) {
					console.log(results[0].geometry.location);
					map.setCenter(results[0].geometry.location);
					var marker = new google.maps.Marker({
						map: map,
						position: results[0].geometry.location
					});
					document.getElementById("google_map").style.visibility = 'visible';
					if(zoom !== undefined) {
						map.setZoom(zoom);
					}
				}
			});
		}

		$(function() {
			map = new google.maps.Map(document.getElementById("google_map"), { zoom: 14, center: new google.maps.LatLng(0, 0), mapTypeControl: false, mapTypeId: google.maps.MapTypeId.ROADMAP });
			codeAddress('Truckgatan 13, Kungälv, Sweden', 15);
			$('#link_swe').click(function() { codeAddress('Truckgatan 13, Kungälv, Sweden', 15); });
			$('#link_usa').click(function() { codeAddress('800 Bellevue Way NE, USA', 14); });
		});
	</script>
