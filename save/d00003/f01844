<script type="text/javascript">
<!--//--><![CDATA[//><!--

jQuery(document).ready(function(){
    jQuery(".slider-main").append(jQuery(".in_main")); 
    jQuery(".in_main",jQuery(".slider-thumb")).remove(); 
    jQuery(".in_main").show();
    jQuery(".slider-thumb").bxSlider({
        slideWidth: 103,
        minSlides: 6,
        maxSlides: 6,
        moveSlides: 1,

        prevSelector: "#slider-prev",
        nextSelector: "#slider-next",
        prevText: "<img src=\"/personal/sites/all/libraries/bxslider/images/left_arrow.png\" />",
        nextText: "<img src=\"/personal/sites/all/libraries/bxslider/images/right_arrow.png\" />",
        onSliderLoad: function() {
            // allow for entire slider to load (including auto-creation of extra as) before setting onclick functions
            jQuery(".slider-thumb a").click(function(i) {
               // get selected index
                var newIndex = jQuery(this).attr("data-slide-index");
                // get element of selected index
                var elem = jQuery(".slider-main .in_main").eq(newIndex).html();
                // set element to main display container
                jQuery(".container-main").html(elem);

                var oldIndex = jQuery(".slider-thumb a.active").attr("data-slide-index");
                if (oldIndex != newIndex) {
                    jQuery(".slider-thumb a").removeClass("active");
                    jQuery(".slider-thumb a[data-slide-index="+newIndex+"]").addClass("active"); // use this method instead to select all
                }
            });

           // jQuery(".slider-thumb a[data-slide-index=0]").click();
            // activate first item in carousel
            jQuery(".slider-thumb a[data-slide-index=0]").addClass("active");
            var elem = jQuery(".slider-main .in_main").eq(0).html();
            jQuery(".container-main").html(elem);
        },
        onSlideBefore: function(jQueryslideElement, oldIndex, newIndex){
            var size = jQueryslideElement.prevObject.length;
            var index = parseInt(jQuery(".slider-thumb a.active").attr("data-slide-index"));

            if (newIndex > oldIndex && 1 == newIndex-oldIndex || // next
                (0 == newIndex && size-1 == oldIndex) // reached the last element
                ) {
                // reached the last element
                if (size <= index+1) {
                    // set to first element
                    index = 0;
                }
                else {
                    index = index+1;
                }
            }
            else if (oldIndex > newIndex || // prev
                (0 == oldIndex && size-1 == newIndex) // reached the first element
                ) {
                // reached the first element
                if (0 > index-1) {
                    // set to last element
                    index = size-1;
                }
                else {
                    index = index-1;
                }
            }

            // select element
            jQuery(".slider-thumb a[data-slide-index="+index+"]").click();
        }
    });
});

//--><!]]>
</script>
