<div class="panel-separator"></div><div class="panel-pane pane-custom pane-5" >
  
      
  
  <div class="pane-content">
     <script>
/* THESE ARE A FEW HELPFUL JS GA FUNCTIONS TO PROCESS TEXT AND URLs */
/** 
	@name gaText
	@desc PROCESS ANY GA LABEL TEXT - remove whitespaces, special characters and make lowercase 
	@author Miha Iancu
	@since May 6, 2013
*/
function gaText(linkText){
	linkText = jQuery.trim(linkText); // remove whitespaces from the beginning and end
	linkText = linkText.replace(/ /g, '_'); // replace all whitespaces inside the string with underscore
	linkText = linkText.replace(/&amp;/g, '_'); // remove &amp; characters
	linkText = linkText.replace(/[^A-Za-z0-9\_]/g, ''); // remove all other characters except for letters, numbers and underscores
	linkText = linkText.toLowerCase(); 
	return linkText;
}
/** 
	@name gaUrl
	@param linkUrl => usually document.location.toString() or link to tag
	@desc PROCESS GA URL:
		=> return LAST PATH STRING(/last_url_string) if inside singtel.com URL
		=> retutn EMAIL ADDRESS IF MAILTO LINK
		=> return full URL if exit link or PDF link
	COMES WITH '/'	
	@author Miha Iancu
	@since May 6, 2013
*/
 function gaUrl(linkUrl){
	var gaLink = '';
	
	if((linkUrl.indexOf('http')>=0 && (linkUrl.indexOf("singtel.com") < 0 || linkUrl.indexOf("singtel.com") > 10)) || linkUrl.indexOf(".pdf") > 0){
		gaLink = '/'+linkUrl;
	}else if(linkUrl.indexOf('mailto:')>=0){
		gaLink = '/'+ linkUrl + linkUrl.slice(6);
	}else{
		gaLink = gaUrlPath(linkUrl);
	}

	return gaLink;
}	
/** 
	@name gaAction
	@param linkUrl => usually document.location.toString()
	@desc PROCESS GA URL to determine link ACTION:
		=> return 'click' if inside singtel.com URL
		=> return 'exit' if outside singtel.com
		=> return 'download' if link is PDF file
		=> return 'email' if link is MAILTO
	@author Miha Iancu
	@since May 6, 2013
*/
function gaAction(linkUrl){
	var gaAction = 'click';
	if(linkUrl.indexOf('http')>=0 && (linkUrl.indexOf("singtel.com") < 0 || linkUrl.indexOf("singtel.com") > 10)){
		gaAction = 'exit';
		
	}
	if(linkUrl.indexOf(".pdf") > 0){
		gaAction = 'download';			
	}
	if(linkUrl.indexOf("mailto:") >= 0){
		gaAction = 'email';			
	}
	return gaAction;
}
/** 
	@name gaUrlPath
	@param linkUrl => usually document.location.toString()
	@desc PROCESS GA URL to determine path for inside links:
		=> return last part of URL for inside singtel links
		
	@author Miha Iancu
	@since May 6, 2013
*/
function gaUrlPath(linkUrl){
	var pos = linkUrl.lastIndexOf('/');
	if(pos>0){
		linkUrl = linkUrl.slice(pos);
	}
	return linkUrl;
}
</script>   </div>

  
  </div>
  </div>
</div>
