  <div class="pane-content">
     <script>
/* THIS IS A JS SCRIPT THAT USES THE DEFAULT TERMS AND CONDITIONS FROM NGP. 
    IT ADDS GA FOR CLICK AND OVERLAY LOAD(fake one).
    IT ASSUMES U HAVE DEFINED gaSection var or defaults to PAGE TITLE
    @TODO: IF LATER ON YOU WANT TO HAVE T&C in any place of the page, please use this custom pane!! 
*/
jQuery(document).ready(function() {
// T&Cs => FOR GA
jQuery('a.detail-tnc-link').click( function(){
    //  click event
    var pageTitle =  jQuery("h1#page-title").text();
    if(!pageTitle ) {
        pageTitle =  jQuery("h1.title").text();
    }
    if(!gaSection){ gaSection =  jQuery("h1#page-title").text(); }
    trackEvent(gaSection +': ' + pageTitle +  '/Footer','click','tnc');
    // load overlay
    trackPageview(window.location.pathname + '/tnc');
});
});
</script>   </div>

  
