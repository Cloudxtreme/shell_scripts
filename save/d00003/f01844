<script type="text/javascript">
<!--//--><![CDATA[//><!--

jQuery(document).ready(function () {  	
	var urlRss = "http://images.apple.com/main/rss/hotnews/hotnews.rss"
	jQuery.jGFeed(urlRss, function (feeds) {
		if(!feeds){ jQuery(".apple_news").hide(); return false;};
		var htmlFeeds = "";
		for(var index=0; index< feeds.entries.length; index++){
			dt = new Date(new Date(feeds.entries[index].publishedDate)-1);
			datePub = getMonth(dt) + " " + dt.getDate()+" "+ dt.getFullYear();
			htmlFeeds += "<div class=\"line-wrapper clearfix\">"
			htmlFeeds += "<p>"+datePub+":";
			htmlFeeds += "<a href=\""+feeds.entries[index].link+"\" target=\"_blank\" onclick=\"trackEvent('"+gaSection+": "+jQuery('h1#page-title').text()+"/applenews','exit','rssfeed/"+feeds.entries[index].link+"');\"> "+feeds.entries[index].title+"</a></p>";
			htmlFeeds += "</div>";
		}
		jQuery(".apple_news").append(htmlFeeds);
	}, 3);
	});
function getWeekDay(date) {
  var days = ["Sun","Mon","Tue","Wed","Thu","Fri","Sat"]
  return days[ date.getDay() ]
}

function getMonth(date){
	var month=new Array();
	month[0]="January";
	month[1]="February";
	month[2]="March";
	month[3]="April";
	month[4]="May";
	month[5]="June";
	month[6]="July";
	month[7]="August";
	month[8]="September";
	month[9]="October";
	month[10]="November";
	month[11]="December";
	
	return month[date.getMonth()];
}

//--><!]]>
</script>
