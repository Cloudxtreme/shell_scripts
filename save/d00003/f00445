				target = target.length && target;
				if (target.length) {
					var sclpos = 30;
					var scldurat = 500;
					var targetOffset = target.offset().top - sclpos;
					$('html,body')
						.animate({scrollTop: targetOffset}, {duration: scldurat, easing: "linear"});
					return false;
				}
			}
		});
	});

