<script type="text/javascript">

    function showCarouselButtons() {
        document.getElementById('left-control').style.display = 'block';
        document.getElementById('right-control').style.display = 'block';
    }

    function hideCarouselButtons() {
        document.getElementById('left-control').style.display = 'none';
        document.getElementById('right-control').style.display = 'none';
    }

    function showColorPictures() {
        var imgElements = document.getElementsByClassName('carousel-image');
        for(var i = 0; i < imgElements.length; i++){
            //do something to each div like
            var imgElem = imgElements[i];
            imgElem.src = imgElem.src.substring(0, imgElem.src.length-7) + ".jpg";
        }
    }

    function showBwPictures() {
        var imgElements = document.getElementsByClassName('carousel-image');
        for(var i = 0; i < imgElements.length; i++){
            //do something to each div like
            var imgElem = imgElements[i];
            imgElem.src = imgElem.src.substring(0, imgElem.src.length-4) + "_bw.jpg";
        }
    }

    $(window).load(function () {
        $('.carousel').carousel({
            interval: 5000
        });
    });
</script>
