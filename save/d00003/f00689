<script>
        function showSearch() {
		if($('#search-768').height() == '0'){
			$('#search-768 #queryAndWords').focus();
	        	$('#search-768').animate({'height' : '50px', 'padding-top' : '15px'}, 300);
			$('#page-h-search-768 a').addClass('hover');
		}else{
			$('#search-768').animate({'height' : '0px', 'padding-top' : '0px'}, 300);
			$('#page-h-search-768 a').removeClass('hover');
		}
	}

	$('body').click(function(e){
		if(!($(e.target).parents('#search-768-hld').length > 0) && $('#search-768').height() > '0'){
			$('#search-768').animate({'height' : '0px', 'padding-top' : '0px'}, 300);
			$('#page-h-search-768 a').removeClass('hover');
		}
	});

	window.onorientationchange = function(){
		var ori = window.orientation;
		if (ori === 90 || ori == -90) {
			$('#search-768 #queryAndWords').blur();
			$('#search-768').css({'height' : '0px', 'padding-top' : '0px'});
			$('#page-h-search-768 a').removeClass('hover');
		}
	}
</script><div id="page-h" class="osobni">
