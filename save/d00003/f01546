    //<![CDATA[
    function rememberme(link)
    {
	    cookie_set('section',link.substr(7));
    }

    function remembering()
    {
	    var link=cookie_get('section');
	    if (link.length>0)
	    {
		    location="/fr/fr/"+link;
	    }
    }

    

    //]]>
</script>

