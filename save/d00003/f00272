$(document).ready(function() {
  $('#bc-home').cycle({ 
    fx: 'fade', 
    speed: 'slow', 
    timeout: 10000, 
    pager: '#bc-home .page-slide',
    next: '#bc-home .next-slide',
    prev: '#bc-home .prev-slide'/*,
    pause: 1 */
  });

  $('#bc-home .resumeButton').click(function() { 
    $('#bc-home').cycle('resume');
    $("#bc-home .resumeButton").addClass('activeSlide');
    $("#bc-home .pauseButton").removeClass('activeSlide');
  });

  $('#bc-home .pauseButton').click(function() { 
    $('#bc-home').cycle('pause');
    $("#bc-home .pauseButton").addClass('activeSlide');
    $("#bc-home .resumeButton").removeClass('activeSlide');
  });
});
</script>

