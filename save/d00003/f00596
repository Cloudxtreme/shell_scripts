		<div id="poll" style="padding: 0px 0px 15px 0px ;float: left;width: 460px;">













  




<script type="text/javascript">
function doPopup(url)
{
	var radios = document.getElementsByTagName('input');
	for (var i=0; i<radios.length; i++) {
		var input = radios[ i ];    
		if (input.type == 'radio' && input.name == "pollopt" && input.checked) {                          
			url = url+('&pollopt='+input.value);
		}
	}
	
	if(url.length > 0){
		window.open(url, 'poll', "status=yes, width=600px, height=500px, resizable=yes, scrollbars=yes")
	}
}

function dolocation(url)
{
	location.href=url;

}
</script>


