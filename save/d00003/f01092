function setUpPage() {
globalNav_on = 'st_main_button';
mainNav_on = 'mainNav_' + (4 -1) + 'Link';
tier1Nav_on = 'menu_' + (11 -1);
tier2Nav_on = 'menu_' + (11 -1) + '_' + (0-1);
subPageNav_on = '';
page = 'lesson2';

if (globalNav_on != '') {
	hilite(globalNav_on, 1);
}
if (mainNav_on != '') {
	hiliteNavMain(mainNav_on, 1);
}
if (tier1Nav_on != '') {
	hiliteNavLeft(tier1Nav_on, 1, 1, 1);
}
if (tier2Nav_on != '') {
	hiliteNavLeft(tier2Nav_on, 1, 2, 1);
}
if (subPageNav_on != '') {
	hiliteNavSub(subPageNav_on, 1);
}
} // /setUpPage()
</script>
