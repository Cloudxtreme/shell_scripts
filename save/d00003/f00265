<script type="text/javascript">
    try
    {
        var IBD = navigator.userAgent;
        var IBD_reg = /(opera)(\/|\s+)(\d)/i;
        var IBD_arr = IBD_reg.exec( navigator.userAgent );
        var IBD_brw = IBD_arr[1];
        var IBD_ver = parseInt(IBD_arr[3]);

        if( IBD_brw == 'Opera' && IBD_ver < 9 )
        {
                document.getElementById("NavBar").innerHTML = "&nbsp;<br>&nbsp;<br>&nbsp;";
                document.write( '<link href="/css/ie.css" rel="stylesheet" type="text/css">' );
        }
