<script type="text/javascript">
$(document).ready(function() {
if ($("#search-XnxnhvJfLyDxNCmPpIoNTKLje-q").val() != "") {
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-clearButton").addClass("visible");
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-searchFieldset").addClass("hasText");
}
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-clearButton").click(function(e){
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-q").val("");
$(this).removeClass("visible");
$('#search-XnxnhvJfLyDxNCmPpIoNTKLje-searchFieldset').removeClass('hasText');
e.preventDefault();
$('#search-XnxnhvJfLyDxNCmPpIoNTKLje-q').focus();
});
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-q").on("keyup", function(e) {
if ($("#search-XnxnhvJfLyDxNCmPpIoNTKLje-q").val() != "") {
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-clearButton").addClass("visible");
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-searchFieldset").addClass("hasText");
} else {
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-clearButton").removeClass("visible");
}
});
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-filterButton").click(function(e){
$(".filterParameter").remove();
$("#search-XnxnhvJfLyDxNCmPpIoNTKLje-searchFieldset").removeClass("filterd");
$(this).hide();
});
});
</script>   
