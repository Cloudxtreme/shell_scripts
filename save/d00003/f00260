var previousMenuItemShowing = jQuery.data(document.body, previousMenuItemShowingKey);
if (previousMenuItemShowing) {
$(previousMenuItemShowing).hide();
}
});
$('#menu-group-1-260064-753176041_header').click(function(event) {
var nextMenuItemContentToShow = '#menu-group-1-260064-753176041_menuItemContent';
if ($(nextMenuItemContentToShow).is(":hidden")) {
var previousMenuItemShowing = jQuery.data(document.body,
previousMenuItemShowingKey);
event.preventDefault();
if (previousMenuItemShowing) {
$(previousMenuItemShowing).hide();
}
jQuery.data(document.body, previousMenuItemShowingKey,
nextMenuItemContentToShow);
$(nextMenuItemContentToShow).show();
}
event.stopPropagation();
});
}
</script>
<script type="text/javascript">
$(document).ready(function(){
// The fontFamily attribute on #propertiesMegaMenu is set by the css to indicate
// that the mega-menus menus should not be shown.
var viewState = $('#propertiesMegaMenu').css('fontFamily');
if (viewState == "property-view-state_maximized") {
$(".block.megaMenu .menu .item").hoverIntent(show, hide);
$(".block.megaMenu .menu .item.noJavascript").removeClass("noJavascript");
}
});
function show(){
var property = $('#propertiesMenuItemContent').css('fontFamily');
if (property == "property-view-state_hide-menu-item-content") {
return;
}
var menuItemContentId = $(this).data("menuItemContentId");
var menuItemContent = $("" + menuItemContentId);
if ($.browser.msie) {
menuItemContent.show();
} else {
menuItemContent.fadeIn(400);
}
}
function hide(){
var menuItemContentId = $(this).data("menuItemContentId");
var menuItemContent = $("" + menuItemContentId);
if ($.browser.msie) {
menuItemContent.hide();
} else {
menuItemContent.fadeOut(200);
}
}
</script> </div>
</nav> 
    
