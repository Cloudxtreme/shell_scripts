var iro2;
var iro3;
var iro4;
var iro5;
var iro6;
var iro7;
var iro8;
var iro9;
var iro10;
var kurokaisu=0;
var akakaisu=0;
var kuroren=1;
var akaren=1;
var kurorensai=1;
var akarensai=1;
var kane=0;
var sihonkin=0;
var sikoukai=0;
var kurokakuritu=0;
var akakakuritu=0;
var kakekin=100;
var kakekingoukei=0;
var shokaif=0;
var atarikin=0;
var zenkaitoonaji=0;
var zenkaikakuritu=0;
var yonkaionaji=0;
var yonkaitigau=0;
var yonkai=0;
var yonkaionajikakuritu;
var yonkaitigaukakuritu;

var speed=0;

function TOKEI(){
sikoukai++;
var iro=Math.floor(2*Math.random());

if(iro==0){

if(iro2=="●"){
zenkaitoonaji++;
}

if(iro2=="●" && iro3=="●" && iro4=="●" && iro5=="●" && iro6=="○"){
yonkaionaji++;
}

if(iro2=="○" && iro3=="○" && iro4=="○" && iro5=="○" && iro6=="●"){
yonkaitigau++;
}

iro="●";
kurokaisu++;
atarikin=atarikin+kakekin*2;
kakekin=100;

	if(iro2=="●"){
	kuroren++;
		if(kuroren > kurorensai){
		kurorensai=kuroren;
		}
	}
	else{
	kuroren=1;
	}
}
else{

if(iro2=="○" && iro3=="○" && iro4=="○" && iro5=="○" && iro6=="●"){
yonkaionaji++;
}

if(iro2=="●" && iro3=="●" && iro4=="●" && iro5=="●" && iro6=="○"){
yonkaitigau++;
}

iro="○";
akakaisu++;
kakekin=kakekin*2;
	if(iro2=="○"){
	akaren++;
zenkaitoonaji++;
		if(akaren > akarensai){
		akarensai=akaren;
		}
	}
	else{
	akaren=1;
	}

if(shokaif==0){
sihonkin=sihonkin-100;
kakekin=kakekin-100;
}
}

if(sihonkin<kakekin){
sihonkin=kakekin;
}

kakekingoukei=kakekingoukei+kakekin;
kane=atarikin-kakekingoukei;

kurokakuritu=sikoukai/kurokaisu;
document.kurokai.kakuritu.value="1/"+kurokakuritu;
akakakuritu=sikoukai/akakaisu;
document.akakai.kakuritu.value="1/"+akakakuritu;

yonkai=yonkaionaji+yonkaitigau;
yonkaionajikakuritu=yonkai/yonkaionaji;
yonkaitigaukakuritu=yonkai/yonkaitigau;

document.iro.n.value=iro;
document.iro2.nn.value=iro2;
document.iro3.nnn.value=iro3;
document.iro4.nnnn.value=iro4;
document.iro5.nnnnn.value=iro5;
document.iro6.nnnnnn.value=iro6;
document.iro7.nnnnnnn.value=iro7;
document.iro8.nnnnnnnn.value=iro8;
document.iro9.nnnnnnnnn.value=iro9;
document.iro10.nnnnnnnnnn.value=iro10;

document.sikou.kaisu.value=sikoukai;
document.kurokai.kaisu.value=kurokaisu;
document.akakai.kaisu.value=akakaisu;

document.kuroren.renzoku.value=kuroren;
document.akaren.renzoku.value=akaren;

document.kurorensai.renzoku.value=kurorensai;
document.akarensai.renzoku.value=akarensai;

document.mouketa.kane.value=kane+"円";
document.hituyou.sihon.value=sihonkin+"円";

document.kakee.kin.value=kakekin+"円";
document.kake.kingoukei.value=kakekingoukei+"円";
document.atari.kin.value=atarikin+"円";

document.yon.kai.value=yonkai;
document.yonkai.onaji.value=yonkaionaji;
document.yonkaii.tigau.value=yonkaitigau;
document.yonkai.kakuritu.value="1/"+yonkaionajikakuritu;
document.yonkaii.kakuritu.value="1/"+yonkaitigaukakuritu;

iro10=iro9;
iro9=iro8;
iro8=iro7;
iro7=iro6;
iro6=iro5;
iro5=iro4;
iro4=iro3;
iro3=iro2;
iro2=iro;

zenkaikakuritu=(sikoukai-1)/zenkaitoonaji;

document.zenkai.onaji.value=zenkaitoonaji;
document.zenkai.kakuritu.value="1/"+zenkaikakuritu;

shokaif=1;

document.close();
setTimeout('TOKEI()',speed);
}
// -->
