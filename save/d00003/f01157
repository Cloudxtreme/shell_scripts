	function validateForm_1( form ){
		var errorArray = new Array();
		clearErrors_1();

		var h81ce = document.getElementById('h81ce_79');
		var h81ce_label = document.getElementById('h81ce_79_label');
		var h81ce_error = document.getElementById('h81ce_79_error');
		if(h81ce.value.length == 0){
			var required = true;
			if(required){
				errorArray.push({id:h81ce,msg:'error'});
				h81ce.className ='input-error ';
				h81ce_label.className ='label-error ';
				h81ce_error.innerHTML='Field Required';
			}
		} else {
		}


		var hd044 = document.getElementById('hd044_80');
		var hd044_label = document.getElementById('hd044_80_label');
		var hd044_error = document.getElementById('hd044_80_error');
		if(hd044.value.length == 0){
			var required = true;
			if(required){
				errorArray.push({id:hd044,msg:'error'});
				hd044.className ='input-error ';
				hd044_label.className ='label-error ';
				hd044_error.innerHTML='Field Required';
			}
		} else {
		}


		var hf3e9 = document.getElementById('hf3e9_81');
		var hf3e9_label = document.getElementById('hf3e9_81_label');
		var hf3e9_error = document.getElementById('hf3e9_81_error');
		if(hf3e9.value.length == 0){
			var required = true;
			if(required){
				errorArray.push({id:hf3e9,msg:'error'});
				hf3e9.className ='input-error ';
				hf3e9_label.className ='label-error ';
				hf3e9_error.innerHTML='Field Required';
			}
		} else {
			var regEx = /^[-.\w]+\@[-.\w]+$/
			if(!regEx.test(hf3e9.value)){ 
				errorArray.push({id:hf3e9,msg:'error'})
				hf3e9.className ='input-error ';
				hf3e9_label.className ='label-error ';
				hf3e9_error.innerHTML='Invalid format';
			}
		}


		var h434f = document.getElementById('h434f_82');
		var h434f_label = document.getElementById('h434f_82_label');
		var h434f_error = document.getElementById('h434f_82_error');
		if(h434f.value.length == 0){
			var required = false;
			if(required){
				errorArray.push({id:h434f,msg:'error'});
				h434f.className ='input-error ';
				h434f_label.className ='label-error ';
				h434f_error.innerHTML='Field Required';
			}
		} else {
		}


		var h1a55 = document.getElementById('h1a55_83');
		var h1a55_label = document.getElementById('h1a55_83_label');
		var h1a55_error = document.getElementById('h1a55_83_error');
		if(h1a55.value.length == 0){
			var required = true;
			if(required){
				errorArray.push({id:h1a55,msg:'error'});
				h1a55.className ='input-error ';
				h1a55_label.className ='label-error ';
				h1a55_error.innerHTML='Field Required';
			}
		} else {
		}


		var hdbc8 = document.getElementById('hdbc8_84');
		var hdbc8_error = document.getElementById('hdbc8_84_error');
		var hdbc8_label = document.getElementById('hdbc8_84_label');
		if(hdbc8.value.length == 0){
			var required = false;
			if(required){
				errorArray.push({id:hdbc8,msg:'error'})
				hdbc8.className ='input-error ';
				hdbc8_label.className ='label-error ';
				hdbc8_error.innerHTML='Field Required';
			}
		} else {
			if( hdbc8.value.length > 500 ){
				errorArray.push({id:hdbc8,msg:'error'})
				hdbc8.className = 'input-error ';
				hdbc8_label.className = 'label-error ';
				hdbc8_error.innerHTML='Too long';
			}
			if( hdbc8.value.length < 0 ){
				errorArray.push({id:hdbc8,msg:'error'})
				hdbc8.className = 'input-error ';
				hdbc8_label.className = 'label-error ';
				hdbc8_error.innerHTML='Too short';
			}
		}





		if(errorArray.length){
			for(i=0;i<errorArray.length;i++){
				
				
				
			}
			return false;
		}
		return true;
	}
