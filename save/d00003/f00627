    try
    {
        var IBD = navigator.userAgent;
        var IBD_reg = /(opera)(\/|\s+)(\d)/i;
        var IBD_arr = IBD_reg.exec( navigator.userAgent );
        var IBD_brw = IBD_arr[1];
