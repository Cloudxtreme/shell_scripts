	<script>
	$(document).ready( function() {
		function checkForm() {
			if ( $('#searchshopping').is(':checked') ) {
				$('#googleBox').css( 'background','none');	
				$('#form1').attr('action','');
			}
			else { 
				$('#googleBox').css( 'background','url("http://img3.free.fr/2008/portail/new/im/new/logoGoogle.png") no-repeat scroll right 4px transparent' );
				$('#form1').attr('action','http://search.free.fr/google.pl'); 
			}		
		}
		
		checkForm();
		
		$('#form1 .radio').change( function() { 
			checkForm();
		});
		
		$('#form1').live('submit', function() {	
			var keyword= $('#textfield').val().replace(/ /g,'+');		
			if ( $('#searchshopping').is(':checked') ) { recordOutboundLink('', "Header HP", "Search Shopping (from header HP)");	 window.location = 'http://shopping.portail.free.fr/s/w/'+escape(keyword); return false; }
			else { recordOutboundLink(this, "Header HP", "Search (from header HP)");} 
		});

	});
	</script>
