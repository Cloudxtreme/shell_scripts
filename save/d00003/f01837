TEST6.query.jsonp({
	url: "http://b-ipv6test.trrsf.com.br/ip/html",
	callback: "html",
	cache: true,
	timeout: 7000,
	success: function(data) {
		if (data.ip === "ipv4") {
			TEST6.query("#addr").text("Seu IP: "+data.addr);
		} else {
			TEST6.query("#addr").text("Seu IP: "+data.addr+" ("+data.tunnel+")");
		}
	}
});
</script>
