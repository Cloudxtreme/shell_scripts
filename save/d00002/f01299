			field_name = new Array("company","name","title","address","city","State","zip","phone","phone2","fax","email","website","month1","day1","year1","time1");
			// verify that all required fields are entered:
			for (i in field_name) {
				if (eval("f." + field_name[i] + ".value")=="") {
					alert("Please enter your " + field_display[i] + ".");
					eval("f." + field_name[i] + ".focus()");
					return false;
				}
			}
			// verify that fields have the correct data type:
			if (!f.zip.value.match(/^\d{5}$/)) {
