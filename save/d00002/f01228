function launchMusic() {	
	var str = "height=514,innerHeight=514";
	str += ",width=770,innerWidth=770";
	if (window.screen) {
    var ah = screen.availHeight - 30;
    var aw = screen.availWidth - 10;

    var xc = (aw - 770) / 2;
    var yc = (ah - 514) / 2;

    str += ",left=" + xc + ",screenX=" + xc;
    str += ",top=" + yc + ",screenY=" + yc;
  }
  str += ",toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0";
music = window.open('http://airandspace.si.edu/music/', 'Music', str);
}
