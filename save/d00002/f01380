          <script>
						//formHallTypeのchange処理で起動
						function funHallSearchChange(){
							//現状選択されているformHallTypeの値で分岐
							if( $('input[name="formHallType"]:checked').val() == "0"){
								$("#topSearchCompanyHallForm").attr("action","./saijyo/searchlist.html");
							}else{
								$("#topSearchCompanyHallForm").attr("action","./kasou/searchlist.html");
							}
						}
          </script>
          </dd>
