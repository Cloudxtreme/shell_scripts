function ocw(anchor,w,h,name,params) {
	sw=screen.availWidth;
	sh=screen.availHeight;
	xpos=(sw-w)/2;
	ypos=(sh-h)/2;
	return window.open(anchor,name,"width="+w+",height="+h+",top="+ypos+",left="+xpos+","+params);
}
