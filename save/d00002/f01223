<script type="text/javascript">

function dipKanae(){
	$("a#head-kanae").addClass("actv");
	$("div#kanae").css("display","block");
	$("a#head-site-search").removeClass("actv");
	$("div#site-search").css("display","none");
}

function dipSitesearch(){
	$("a#head-kanae").removeClass("actv");
	$("div#kanae").css("display","none");
	$("a#head-site-search").addClass("actv");
	$("div#site-search").css("display","block");
}

</script>







