<script type="text/javascript">
	function show_file2(media_resource_id, file_type, path) {
	   //alert('1234');
	   window.open("/stockexchange_file.php?file_="+media_resource_id+"&file_type_="+file_type,"Image","left=20,top=20,toolbar=no,location=no,menubar=no,resizable=yes,scrollbars=no,width=640,height=480"); 
		return false;	   
	}

	function show_file3(media_resource_id, file_type, path) {
