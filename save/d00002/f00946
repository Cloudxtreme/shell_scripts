                    		<script type="text/javascript">
		function gId(id) {
			if (document.getElementById) return document.getElementById(id);
			else return false;
		}
		var tab = 'internet';
		function showOusko2(id) {
			if (tab == id) return false;
			gId('search_input_'+id).value = gId('search_input_'+tab).value;
			gId('search_container_'+tab).style.display = "none";
			gId('search_container_'+id).style.display = "block";
			gId('search_input_'+id).focus();
			tab = id;
			return false;
		}

		</script>

