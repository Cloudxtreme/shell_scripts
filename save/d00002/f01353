		// custom tracking URL
		var lHash = window.location.hash;
		var lSearch = window.location.search;

		if (lHash.substr(0, 4) != '#ic=') {
			return gPair;
		}
		// convert hash params to regular
		gPair.push(window.location.pathname + lSearch
			+ (lSearch.length == 0 ? '?' : '&') + lHash.substr(1));

		return gPair;
	})());

