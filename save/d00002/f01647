	<script type="text/javascript">
	$("a.spamReport").live("click", function () {
		if (confirm("Jste si jist, že chcete tento příspěvek nahlásit jako spam?")) {
			reloadComponent(this);
			alert('Vaše upozornění bylo odesláno, děkujeme.');
		}
		return false;
	});
	var isMovingAllowed = true;
	$('input[type=text],textarea,select').focus(function () {
		isMovingAllowed = false;
	}).blur(function () {
		isMovingAllowed = true;
	})
	$("body").keydown(function(e) {
		if (isMovingAllowed) {
						if(e.keyCode != 78 && e.keyCode != 80) {
				return;
			}
						if(!document.location.hash.match(/^#n[0-9]+$/)) {
				var current = 0;
			} else {
				var current = parseInt(document.location.hash.replace(/^#n/, ''));
			}
						if(e.keyCode == 80) {
				current--;
			} else {
				current++;
			}
						var link = $('#opl > a[name=n'+current+']:first');
			if(link.length) {
				document.location.hash = link.attr('name');
			}
		}
	});
	</script>

