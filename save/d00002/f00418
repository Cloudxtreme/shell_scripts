<script type="text/javascript" language="javascript">
function doSection(secNum) {

   if (bDoesDHTML) {
      if(eval(secNum).className=="off"){
         eval(secNum).className="on";
         if(secNum=="connection"){
            document.form_.ch_connection_.checked = true;
         }
         if(secNum=="account"){
            document.form_.ch_manage_.checked = true;
         }
         if(secNum=="vas"){
            document.form_.ch_value_added_services_.checked = true;
         }
         if(secNum=="roaming"){
            document.form_.ch_roaming_.checked = true;
         }
         if(secNum=="surfing"){
            document.form_.ch_surfing_.checked = true;
         }
         if(secNum=="email"){
            document.form_.ch_email_.checked = true;
         }
         
      }else {
         eval(secNum).className="off"
      }
   }
}
/* <![CDATA[ */ 
                                 // check for empty field
                                    window.onload = promptMesg;
                                    
                                                                        
                                   function max_count() 
                                    {
                                    	if (document.getElementById("your_comments_").value.length > 7000)
                                    	{
                                    		document.getElementById("your_comments_").value = document.getElementById("your_comments_").value.substring(0, 7000);
                                    	}
                                    	document.getElementById("charstyped").value=7000-document.getElementById("your_comments_").value.length;   
                                    }
                                    
                                    function req_max_count() 
                                    {
                                    	if (document.getElementById("your_requirements_").value.length > 7000)
                                    	{
                                    		document.getElementById("your_requirements_").value = document.getElementById("your_requirements_").value.substring(0, 7000);
                                    	}
                                    	document.getElementById("charstyped").value=7000-document.getElementById("your_requirements_").value.length;   
                                    }
                                    
                                    function username_max_count() 
                                    {
                                    	if (document.getElementById("username_").value.length > 16)
                                    	{
                                    		document.getElementById("username_").value = document.getElementById("username_").value.substring(0, 16);
                                    	}
                                    	document.getElementById("username_charstyped").value=16-document.getElementById("username_").value.length;   
                                    }
                                    
                                    function address_max_count()
                                    {
                                        if (document.getElementById("address_").value.length > 100)
                                    	{
                                    		document.getElementById("address_").value = document.getElementById("address_").value.substring(0, 100);
                                    	}
                                    	document.getElementById("address_charstyped").value=100-document.getElementById("address_").value.length;
                                    }
                                    
                                    function wsurf_zone_max_count()
                                    {
                                        if (document.getElementById("wsurf_zone_").value.length > 50)
                                    	{
                                    		document.getElementById("wsurf_zone_").value = document.getElementById("wsurf_zone_").value.substring(0, 50);
                                    	}
                                    	document.getElementById("wsurf_zone_charstyped").value=50-document.getElementById("wsurf_zone_").value.length;
                                    }
                                    
                                    function wsurf_zone_loc_max_count()
                                    {
                                        if (document.getElementById("wsurf_zone_loc_").value.length > 50)
                                    	{
                                    		document.getElementById("wsurf_zone_loc_").value = document.getElementById("wsurf_zone_loc_").value.substring(0, 50);
                                    	}
                                    	document.getElementById("wsurf_zone_loc_charstyped").value=50-document.getElementById("wsurf_zone_loc_").value.length;
                                    }
                                    
                                    function error_message_max_count()
                                    {
                                        if (document.getElementById("error_message_").value.length > 250)
                                    	{
                                    		document.getElementById("error_message_").value = document.getElementById("error_message_").value.substring(0, 250);
                                    	}
                                    	document.getElementById("error_message_charstyped").value=250-document.getElementById("error_message_").value.length;
                                    }
                                    
                                    function watermark()
                                    {
                                        if (document.getElementById("your_requirements_").value == "Your Requirements")
                                            document.getElementById("your_requirements_").value = "";
                                        else
                                        {
                                            if (document.getElementById("your_requirements_").value.length == 0)
                                                document.getElementById("your_requirements_").value = "Your Requirements";
                                        }
                                    }
                                    
                                    function clearform(){
                                    	//window.location.reload();
                                    	document.form_.name_.value = "";
                                    	document.form_.salutation_.selectedIndex = 0;
                                    	document.form_.contact_number_.value = "";
                                    	document.form_.email_.value = "";
                                    	document.form_.nature_of_feedback_.value = "";
                                    	document.form_.commenting_on_.value = "";
                                       
                                    	                                                                                
                                    	                                    
                                       
                                    	                                    		if(document.form_.service_number_) {
                                       			document.form_.service_number_.value = "";
                                    		}
                                    	                                       
                                    	                                       		if(document.form_.your_comments_) {
                                       			document.form_.your_comments_.value = "";
                                       		}
                                    	                                       
                                    	                                    
                                    	                                    
                                    	                                    	
                                    	                                    	
                                    	                                    
                                                                            }
                                    
                                    
                                    // form checking function
                                    function formcheck() {
                                    	
                                    	if(document.getElementsByName("custType1_")[0].checked == false && document.getElementsByName("custType1_")[1].checked == false)
                                    	{
                                    	    alert("Please select \'Customer Type\'.")
                                            return false;
                                    	}
                                    	
                                    	if(document.form_.salutation_.selectedIndex == 0) {
                                    	   alert("Please select \'Salutation\'.")
                                    	   document.form_.salutation_.focus();
                                    	   return false;
                                    	}
                                    
                                    	// checks name  (empty)
                                    	if (!(checkempty(document.form_.name_, "name"))) {
                                    		return false;
                                    	}
                                    	
                                    	                                    	    // checks NRIC/FIN  (empty)
                                    	    if (document.getElementsByName("custType1_")[0].checked)
                                    	    {
                                    	        if (!(checkempty(document.form_.cust_id_, "NRIC/FIN"))) {
                                    		        return false;
                                    	        }
                                    	    }
                                    	                                       
                                    	// checks contact number  (empty)
                                    	if (!(checkempty(document.form_.contact_number_, "contact number"))) {
                                    		return false;
                                    	}else {
                                    	    if (document.form_.contact_number_.value.length < 8)
                                    	    {   
                                    	        alert("Please enter a valid contact number.")
                                    		    document.form_.contact_number_.focus();
                                    		    document.form_.contact_number_.select();
                                    		    return false;
                                    	    }
                                    	    else
                                    	    {
                                    		    if (!(isContactNumber(document.form_.contact_number_.value))) 
                                    		    {
                                    			    alert("Please enter a valid contact number.")
                                    			    document.form_.contact_number_.focus();
                                    			    document.form_.contact_number_.select();
                                    			    return false;
                                    		    }
                                            }
                                    	}
                                       	
                                    	// checks email address  (empty)
                                    	if (!(checkempty(document.form_.email_, "email address"))) {
                                    		return false;
                                    	}
                                    	
                                    	// checks email address  (email format)
                                    	if (!(checkoneemail(document.form_.email_, "email address"))) {
                                    		return false;
                                    	}
                                    	
                                    
                                    	                                    
                                    	if (document.form_.nature_of_feedback_.selectedIndex == 0 ){
                                    		alert("Please select nature of feedback.");
                                    		document.form_.nature_of_feedback_.focus();
                                    		return false;	   
                                    	}
                                                                       
                                    	if (document.form_.commenting_on_.selectedIndex == 0 ){
                                    		alert("Please select subject.");
                                    		document.form_.commenting_on_.focus();
                                    		return false;	   
                                    	}
                                                                        	
                                    	// Hello Shops
                                    	                                    	// ************************************************************************
					// SingTel My Account
                                                                            //*********************************************************************
                                        // exStream Fibre Services
                                         	
                                    	// Wireless@SG
                                    	                                    	
                                    	// International Calls (IDD, STD, ICC)
                                    	                                    	// ****************************************************************
                                    	
                                    	// singtelshop.com
                                    	                                    	// ****************************************************************
                                    	
                                	// Mio Plan
                                    	 // end of Mio Plan
                                        // ****************************************************************
                                    	
                                    	// Mio TV
                                    	                                        // ****************************************************************
                                    	
                                    	// Mobile / IDEAS
                                    	                                    	// ****************************************************************
                                        
                                        // Bills
                                                                            	// ****************************************************************
                                    	
                                        // Telephone
                                    	                                       // ****************************************************************
                                        
                                        // Internet
                                                                            	
                                    	// SingNet
                                    		//end of singnet
                                    	
                                    	                                       		if(document.form_.your_comments_){
                                       			if (!(checkempty(document.form_.your_comments_, "comments"))) {
                                    				return false;
                                    			}
                                    			else{
                                    				if (document.form_.your_comments_.value.length > 7000)
                                    				{
                                    					alert("Please submit your comments in less than 7000 characters.");
                                    					document.form_.your_comments_.focus();
                                    					return false;
                                    				}
                                    			}
                                    		}
                                       									   									   
	document.getElementById("submit_btn").disabled = true;				                                                                                                                                document.getElementById("form_").action = '/form_feedback_process.php';
                                                                                                                                document.getElementById("form_").submit();
			   
                                        
                                    	
                                    }
                                    
                                    function promptMesg()
                                    {
									                                    }
/* ]]> */                               
</script>

