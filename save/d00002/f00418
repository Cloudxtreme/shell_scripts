    function popup(file_name){
        form_window = window.open(file_name, 'popup', 'scrollbars,width=430,height=400,status=no,toolbar=no,menubar=no,location=no,resizable=no');
        form_window.focus();
    }
    
    function popup_customise_message(file_name){
        form_window = window.open(file_name, 'popup', 'scrollbars,width=500,height=400,status=no,toolbar=no,menubar=no,location=no,resizable=yes');
        form_window.focus();
    }
