function TOKEI(){

var iti=Math.floor(3*Math.random());
var nii=Math.floor(3*Math.random());
var san=Math.floor(3*Math.random());
var sii=Math.floor(3*Math.random());
var goo=Math.floor(3*Math.random());
var rok=Math.floor(3*Math.random());
var nan=Math.floor(3*Math.random());
var hat=Math.floor(3*Math.random());
var kyu=Math.floor(3*Math.random());
var jyu=Math.floor(3*Math.random());
var jyi=Math.floor(3*Math.random());
var jyn=Math.floor(3*Math.random());
var jys=Math.floor(3*Math.random());
var jyy=Math.floor(3*Math.random());


document.iti.ber.value=iti;
document.ni.ber.value=nii;
document.san.ber.value=san;
document.yon.ber.value=sii;
document.go.ber.value=goo;
document.roku.ber.value=rok;
document.nana.ber.value=nan;
document.hati.ber.value=hat;
document.kyu.ber.value=kyu;
document.jyu.ber.value=jyu;
document.jyuiti.ber.value=jyi;
document.jyuni.ber.value=jyn;
document.jyusan.ber.value=jys;
document.jyuyon.ber.value=jyy;


kane=kane-300;

kounyu=kounyu+300;
maisuu++;
document.kou.nyu.value=kounyu+"円";



if(iti==itisiai){
ittisuu++;
}
if(nii==niisiai){
ittisuu++;
}
if(san==sansiai){
ittisuu++;
}
if(sii==yonsiai){
ittisuu++;
}
if(goo==gosiai){
ittisuu++;
}
if(rok==rokusiai){
ittisuu++;
}
if(nan==nanasiai){
ittisuu++;
}
if(hat==hatisiai){
ittisuu++;
}
if(kyu==kyusiai){
ittisuu++;
}
if(jyu==jyusiai){
ittisuu++;
}
if(jyi==jyuitisiai){
ittisuu++;
}
if(jyn==jyunisiai){
ittisuu++;
}
if(jys==jyusansiai){
ittisuu++;
}
if(jyy==jyuyonsiai){
ittisuu++;
}


//一等判定
if(ittisuu==14){

kane=kane+600000000;
tousen=tousen+600000000;

atnaiyous=atnaiyoun;
atnaiyoun=atnaiyoui;
atnaiyoui="☆一等6億円！！！☆";
alert("一等だよ!!!!");
ittouate++;
}

//二等判定
if(ittisuu==13){

kane=kane+2562300;
tousen=tousen+2562300;

atnaiyous=atnaiyoun;
atnaiyoun=atnaiyoui;
atnaiyoui="二等2562300円当たり!";
alert("二等だよ!!");
nitoate++;
}

//三等判定
if(ittisuu==12){

kane=kane+78840;
tousen=tousen+78840;

atnaiyous=atnaiyoun;
atnaiyoun=atnaiyoui;
atnaiyoui="三等78840円当たり！！";
alert("三等だよ");
santoate++;
}

//四等判定
if(ittisuu==11){
kane=kane+9855;
tousen=tousen+9855;
atnaiyous=atnaiyoun;
atnaiyoun=atnaiyoui;
atnaiyoui="四等9855円当たり！";

if(yonara==0){
alert("四等だよ");
}
yontoate++;
}

//五等判定
if(ittisuu==10){
kane=kane+1794;
tousen=tousen+1794;
atnaiyous=atnaiyoun;
atnaiyoun=atnaiyoui;
atnaiyoui="五等1794円当たり！";
gotoate++;
}



document.atari.i.value=atnaiyoui;
document.atarini.n.value=atnaiyoun;
document.atarisa.nn.value=atnaiyous;

document.tou.sen.value=tousen+"円";

document.mo.ney.value=kane+"円";

document.ittou.suu.value=ittouate+"/"+maisuu;
document.ittou.ji.value="1/"+maisuu/ittouate;

document.nito.suu.value=nitoate+"/"+maisuu;
document.nito.ji.value="1/"+maisuu/nitoate;

document.santo.suu.value=santoate+"/"+maisuu;
document.santo.ji.value="1/"+maisuu/santoate;

document.yonto.suu.value=yontoate+"/"+maisuu;
document.yonto.ji.value="1/"+maisuu/yontoate;

document.gotoo.suu.value=gotoate+"/"+maisuu;
document.gotoo.ji.value="1/"+maisuu/gotoate;


ittisuu=0;


document.close();
setTimeout('TOKEI()',speed);
}
// -->
