<script>
	function start() {
		var datesel =document.getElementById('selectDateProcess').value
		document.getElementById('selectDate').value=datesel;
	}
	
	if (typeof window.addEventListener != 'undefined') 
	{
		window.addEventListener(
		'load',
		function (evt) {
		start();
		},
		false
		);
	}
	else if (typeof window.attachEvent != 'undefined') 
	{
		window.attachEvent(
			'onload',
			function () {
			start();
			}
			);
	}
</script>


