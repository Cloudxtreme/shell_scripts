<script type="text/javascript">
	function rebindFaceboxEvent(){
		   $('#facebox .body').css({"background":"#000"});
		$(document).bind('reveal.facebox', function() {
			if (window.addthis){
	         //window.addthis.ost = 0;
	         //window.addthis.ready();
	         window.addthis.button('.shareButton');
	         window.addthis.addEventListener('addthis.menu.share',
	fnShareEloqua);
					}
				});
	}

	function checkAll(){ 
  		
	$('input[type=checkbox]').checkBox('changeCheckStatus', true);
	}

	var page = "";
	var dateRange = "";

	var loaded = '&loaded=false';
	function getPagination(y) {
		page = "&page=" + y;
		get_check_value();

	}
	var url = "/content/traveller/en/browse/whats-on/jcr:content.event_calendar_monthlyview.xyz?1=1";
	var selectedTag = "";
	if (selectedTag.length > 0) {
		url = url + "&selected=" + selectedTag;
	}
	var selected = "";
	function get_check_value() {

		var dateranges = document.getElementById('selectDateProcess').value;

		dateRange = "&selectDateProcess=" + dateranges;

		var c_value = "";
		var j = 0;
		var count = document.getElementById('tagListSize').value;

		for ( var i = 0; i < count; i++) {
			if (document.getElementById(('tag' + i)).checked) {
				j++;
				if (j == 1) {
					c_value = c_value
							+ document.getElementById(('tag' + i)).value;
				} else {
					c_value = c_value + ","
							+ document.getElementById(('tag' + i)).value;
				}
			}
		}

		selected = "&selected=" + c_value;
		url = url + selected + page + dateRange + loaded;
		var val = document.getElementById('desc').innerHTML;
		setResult(val);
		$.get(url, function(data) {
			document.getElementById('searchListing').innerHTML = data; 
	
	//  var obj =  document.form[0].first;
			//  alert(".1");
			//   alert(document.getElementById('first').value+".1");
			//if ( obj!=null ){
			var test = document.getElementById('first').value;
			setResult(test);
			//}
		});
		url = "/content/traveller/en/browse/whats-on/jcr:content.event_calendar_monthlyview.xyz?1=1";
		selected = "";
		page = "";
		clear = "";
		loaded = "";

	}

	$.get(url, function(data) {
		document.getElementById('searchListing').innerHTML = data;
		// var obj = document.form[0].first;
		// alert(".2");
		//     alert(document.getElementById('first').value+".2");
		if (document.getElementById('first')) {
			var test = document.getElementById('first').value;
			setResult(test);
		}
		get_check_value();
		//   alert(".3");
		//    alert(document.getElementById('first').value+".3");
		if (document.getElementById('first')) {
var test = document.getElementById('first').value;
       		setResult(test);
       }
       
   } 
   
);
 
function getDateRange(){

   checkAll(); 
   get_check_value();  
}

function setResult(str)
{
		 if(document.getElementById("desc")){
		 
		 	document.getElementById("desc").innerHTML = str;
		 }
}

</script>


