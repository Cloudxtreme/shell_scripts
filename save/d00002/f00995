<script type="text/javascript">
function resizesHomeCentreBand()
{
	var var1 = (($('.wrapper').width() - $('#home-centre-band').width())/2) + 'px';
	$('#home-centre-band').css('margin-right', '-' + var1);
	$('#home-centre-band').css('margin-left', '-' + var1);
	$('#home-centre-band').css('padding-right', var1);
	$('#home-centre-band').css('padding-left', var1);
}
ExecuteOrDelayUntilScriptLoaded(function () { SP.UI.Workspace.add_resized(resizesHomeCentreBand); }, "init.js");
</script>


