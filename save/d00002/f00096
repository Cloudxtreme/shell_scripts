<script type="text/javascript">
    function toggleType(type){
        var selector = '#prog-wrapper';
        var newclass = "hide" + type;
        if($(selector).attr('class').indexOf(newclass)!=-1){
            $(selector).removeClass(newclass);
        }
        else{
            $(selector).addClass(newclass);
        }
    }
    function displayType(){
        $('#typeapnic').parent().prepend('<input type="checkbox" checked="checked" onclick="toggleType(\'apnic\')"  />');
        $('#typeapricot').parent().prepend('<input type="checkbox" checked="checked" onclick="toggleType(\'apricot\')"  />');
        $('#typesocial').parent().prepend('<input type="checkbox" checked="checked" onclick="toggleType(\'social\')"  />');
        $('#typeinvite').parent().prepend('<input type="checkbox" checked="checked" onclick="toggleType(\'invite\')"  />');
        $('#typebroadcast').parent().prepend('<input type="checkbox" onclick="toggleType(\'broadcast\')"  />');
    }
    displayType();
    function programZoom(){
        document.getElementById('prog-wrapper').innerHTML+="<p style='position:absolute;top:3.5em;right:2em;font-size:11px !important;'><input type='radio' name='progview' value='standard' onclick='toggleZoom(this)'/>Large text | <input type='radio' name='progview' value='wide' checked='checked' onclick='toggleZoom(this)'/>Small text</p>"; 
    }
    function toggleZoom(e){
       if(e.value=='wide'){
           document.getElementById('prog-wrapper').className='wide';
           $('#prog-wrapper').addClass('wide');
       }
       else if(e.value=='standard'){
           $('#prog-wrapper').removeClass('wide');
       }
    }
    //programZoom();
</script>

