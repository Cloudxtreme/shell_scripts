          <li class="field-item odd">
        <a href="/2013/tags/slides" typeof="skos:Concept" property="rdfs:label skos:prefLabel">slides</a>      </li>
          <li class="field-item even">
        <a href="/2013/tags/videos" typeof="skos:Concept" property="rdfs:label skos:prefLabel">videos</a>      </li>
      </ul>
</section>
