							<OPTION VALUE="senjutsu03" >四柱推命</OPTION>
							<OPTION VALUE="senjutsu04" >姓名判断</OPTION>
							<OPTION VALUE="senjutsu05" >タロット</OPTION>
							<OPTION VALUE="senjutsu06" >風水・手相</OPTION>
							<OPTION VALUE="senjutsu07" >心理・診断</OPTION>
							<OPTION VALUE="senjutsu08" >その他の占術</OPTION>
							<OPTION VALUE="senjutsu09" >直接鑑定</OPTION>
					</SELECT>&nbsp;&nbsp;
