	<script type="text/javascript">
		function MenuOver(obj) {
			img = obj.src.split(".");
			index = img.length-2;
			if(img[index].substr((img[index].length-3),3) == "_on") {
				img2 = img[index].substr(0,(img[index].length-3));
			} else {
				img2 = img[index]+"_on";
			}
			obj.src = obj.src.replace(img[index],img2);
		}

		function ReColor(obj) {
			if (obj.className == "" || obj.className == "mouseOut") {
			    obj.className = "mouseOver";
			}
			else
			{
			    obj.className = "mouseOut";
			}

		}
		
		function ChangeInputField()
		{
			document.getElementById('lame_heslo').style.display = '';
			document.getElementById('pass_field').style.display = 'none';
		}

		// complete cross-browser solution to fix window.onload problem

		function init() {
		  // quit if this function has already been called
		  if (arguments.callee.done) return;

		  // flag this function so we don't do the same thing twice
		  arguments.callee.done = true;

		  // kill the timer
		  if (_timer) clearInterval(_timer);

		  document.getElementById('lame_heslo').style.display = '';
			document.getElementById('pass_field').style.display = 'none';

		};

		/* for Mozilla/Opera9 */
		if (document.addEventListener) {
		  document.addEventListener("DOMContentLoaded", init, false);
		}

		/* for Internet Explorer */
		/*@cc_on @*/
		/*@if (@_win32)
		  document.write("<script id=__ie_onload defer src=javascript:void(0)><\/script>");
		  var script = document.getElementById("__ie_onload");
		  script.onreadystatechange = function() {
		    if (this.readyState == "complete") {
		      init(); // call the onload handler
		    }
		  };
		/*@end @*/

		/* for Safari */
		if (/WebKit/i.test(navigator.userAgent)) { // sniff
		  var _timer = setInterval(function() {
		    if (/loaded|complete/.test(document.readyState)) {
		      init(); // call the onload handler
		    }
		  }, 10);
		}

		/* for other browsers */
		window.onload = init;
		
	</script>
