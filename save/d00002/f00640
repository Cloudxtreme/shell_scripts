  <script type="text/javascript">
    $(function() {
      var $section = $('#bg SECTION');
      var $fader = $([]);
      var timer = null;
      function fade(newClass, removeClasses, callback) {
        if($fader.length < 1) {
          $fader = $('<div/>').css({ position: 'absolute', top: 0, left: 0, right: 0, bottom: 0, 'background-repeat': 'no-repeat' }).hide();
          $section.append($fader);
        }
        $fader.stop();
        $fader.css({ 'background-position': $section.css('background-position'), 'background-image': $section.css('background-image') }).show();
        $section.removeClass(removeClasses);
        $section.addClass(newClass);
        $fader.fadeOut(900, callback);
      }
      function eu() { clearTimeout(timer); timer = setTimeout(function() { fade('eu', '', us); }, 3000); }
      function us() { clearTimeout(timer); timer = setTimeout(function() { fade('', 'eu', eu); }, 3000); }

      $section.delegate('.button', 'click', function(event) {
        event.preventDefault();
        var newClass = this.className.split(' ')[1];
        if(!$section.hasClass(newClass)) {
          $section.removeClass('general global power control eu');
          clearTimeout(timer);
          $section.addClass(newClass);
          if(newClass == 'global') {
            eu();
          }
        }
      });
          });
  </script>
