<script type="text/javascript">
$(document).ready(function() {
if ($("#search-OBPTxUEgneaOhCqDHuhCwYUFp-q").val() != "") {
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-clearButton").addClass("visible");
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-searchFieldset").addClass("hasText");
}
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-clearButton").click(function(e){
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-q").val("");
$(this).removeClass("visible");
$('#search-OBPTxUEgneaOhCqDHuhCwYUFp-searchFieldset').removeClass('hasText');
e.preventDefault();
$('#search-OBPTxUEgneaOhCqDHuhCwYUFp-q').focus();
});
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-q").on("keyup", function(e) {
if ($("#search-OBPTxUEgneaOhCqDHuhCwYUFp-q").val() != "") {
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-clearButton").addClass("visible");
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-searchFieldset").addClass("hasText");
} else {
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-clearButton").removeClass("visible");
}
});
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-filterButton").click(function(e){
$(".filterParameter").remove();
$("#search-OBPTxUEgneaOhCqDHuhCwYUFp-searchFieldset").removeClass("filterd");
$(this).hide();
});
});
</script>   
