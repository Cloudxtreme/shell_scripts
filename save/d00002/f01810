	baseicon.shadow = "http://www.google.com/mapfiles/shadow50.png";
	baseicon.iconSize = new GSize(50, 64);
	baseicon.shadowSize = new GSize(57, 54);
	baseicon.iconAnchor = new GPoint(29, 54);
	baseicon.infoWindowAnchor = new GPoint(29, 22);
	baseicon.infoShadowAnchor = new GPoint(38, 45);

// CreateMarkerPoint = Cria um marcador no ponto indicado, utilizando a tabela de informaes desejada
        function createMarkerPoint (point,tabela,icontipe) {
	  var marker = new GMarker(point, icontipe); 
	  GEvent.addListener(marker, "click", function() {
          marker.openInfoWindowTabsHtml(tabela);});
          return marker;
        }

