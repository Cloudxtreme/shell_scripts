    //<![CDATA[

    function load() {
      if (GBrowserIsCompatible()) {
        var map = new GMap2(document.getElementById("map"));
        //Duas prximas linhas colocam os controles de direcao e zoom respectivamente
        map.addControl(new GSmallMapControl());
        map.addControl(new GMapTypeControl());
	 map.addControl(new GOverviewMapControl());
        //Cria o mapa na coordenada referenciada
        //Nesse caso, no centro do RS
        map.setCenter(new GLatLng(-30,-53.278500), 6, G_HYBRID_MAP);
    //]]>

// CreateMarker = Cria um marcador no ponto indicado,utiliznaod a tabela de informaes desejada
	function createMarker(point,tabela) {
	  var marker = new GMarker(point);
	  GEvent.addListener(marker, "click", function() {
	  marker.openInfoWindowTabsHtml(tabela);});
	  return marker;
	}

// criao do " baseIcon", marcador diferente para o POP-RS
	var baseicon = new GIcon();
// marcador POP-RS desenhado no fireworks - azul
