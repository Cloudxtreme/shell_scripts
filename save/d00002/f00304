}



function junpsmp(){
location.href="/smp/supinfo/";

	initSpmode();

}

function initSpmode(){
	document.cookie="sp2pc=;expires=Tue, 1-Jan-1980 00:00:00;;path=/;";

	setSpmode();

}

function setSpmode(){
	devicetime=new Date();
	devicetime.setTime(devicetime.getTime()+(1*1000*60*60*2));
	setdevicetime=devicetime.toGMTString();
	document.cookie="sp2pc=0;expires="+setdevicetime+";path=/;";
}


</script>
