<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js" type="text/javascript"></script>
<script>
  jQuery(document).ready(function(e) {

    jQuery(".customs_searchBtn").click(function(){
		var searchScope = $(".SearchTextBox").val();
		if($('#stesearch').is(':checked')) {
			searchScope = "customs"
		}
		var searchKeyword = $("#customs_search").val();
		var searchResultUrl = "";
		if (jQuery.trim(searchKeyword) == "") return;
		if (searchScope == "customs") {
			searchResultUrl = "http://www.google.com/cse?cx=015907626688418232955:tza9drwnhra&q="+searchKeyword;
		}
		if (searchScope == "default") {
			searchResultUrl = "http://www.google.com/cse?cx=015907626688418232955:5denqx4vlfm&q="+searchKeyword;
		}
		if (searchScope == "within_biz") {
			searchResultUrl = "http://www.google.com/cse?cx=015907626688418232955:a8adudjwolo&q="+searchKeyword;
		}
		if (searchScope == "within_cit_res") {
			searchResultUrl = "http://www.google.com/cse?cx=015907626688418232955:x6djczjjpgc&q="+searchKeyword;
		}
		if (searchScope == "within_non_res") {
			searchResultUrl = "http://www.google.com/cse?cx=015907626688418232955:bcnuxmrmlxs&q="+searchKeyword;
		}
		window.open(searchResultUrl);
    }) 
    

	jQuery("#stesearch, .SearchTextBox, #customs_search").keypress(function(e){
		if (e.which == 13) {
			e.preventDefault();
			jQuery(".customs_searchBtn").click();
		}	
    })
	
	jQuery("#clearRadio").click(function(){
		jQuery("#stesearch").removeAttr('checked');

	})
    
})
  
  
</script>

