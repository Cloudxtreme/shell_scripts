function setParam(node, fi) {
    var id = node.id; 
    if(isNaN(id)){
        id = 0;
    }
    fi.loader.dataUrl = "/content/traveller/tag/tagSearch.taxonomy.json?nodeId="+ id + "&path=/content/traveller/en/browse/aboutsingapore";
}

