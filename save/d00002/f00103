					//generic functions, should be in a separate JS
					 function randomString(string_length) {
						 var chars = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";
						 var randomstring = '';
						 for (var i=0; i<string_length; i++) {
							 var rnum = Math.floor(Math.random() * chars.length);
							 randomstring += chars.substring(rnum,rnum+1);
						 }
						 return randomstring;
					 }
					
					function trim(stringToTrim) {
						return stringToTrim.replace(/^\s+|\s+$/g,"");
					}
					
					function removeNL(s){ 
					  return s.replace(/[\n\r\t]/g,""); 
					}
					
					function submitrating(url){
						var serverurl = "content/traveller/en/browse/aboutsingapore/jcr:content.like";
						$.get(serverurl,{action:"2",likeid:url,rndt:randomString(6)},
							function(data){
								refreshVal(data);
							}
						);
					}
					
					function refreshVal(retval){
						document.getElementById('ratingcount').innerHTML = trim(removeNL(retval));
					}
					
					</script> 	
