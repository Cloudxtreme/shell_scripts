<script type="text/javascript">
	var ads = ['ad-text', 'ad-text2'];
	var adsContents = [];

	(function(window,undefined) {
		var History = window.History;
		var date = new Date();

		$('#calendar a.ajax').live('click', function(e) {
			var $link = this;
			e.preventDefault();

			// clone ads non-script content to later replace
			for (var i in ads) {
				$element = $('#' + ads[i] + ' .ad');
				if ($element.length) {
					adsContents[i] = $element.clone();
				}
			}

			return reloadComponent($link
					+ ($link.toString().indexOf('?') == -1 ? '?' : '&')
					+ '_t=' + date.getTime(),
					function(data) {

				var _doc = $('<div id="_calendar">' + data.snippets.calendar
					+ '</div>');

				// restore ads backup to positions of placeholder elements
				for (i in ads) {
					var $new = $('<div class="textbox" id="' + ads[i] + '">');
					if (typeof adsContents[i] !== 'undefined') {
						$new.append(adsContents[i]);
					}
					$('#placeholder_' + ads[i], _doc).replaceWith($new);
				}

				// modify snippets data directly to look transparent to even
				// History and Nette
				data.snippets.calendar = _doc.html();
				if (History.enabled) {
					History.pushState(data, null, $link);
				}
				$.nette.success(data);
			});
		});

		History.Adapter.bind(window, 'statechange', function() {
			var State = History.getState();

			if (typeof State.data !== 'undefined') {
				$.nette.success(State.data);
			}
		});
	})(window);
</script>

