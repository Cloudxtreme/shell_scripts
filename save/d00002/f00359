         <script language="JavaScript" type="text/javascript">

function getElementsByClass(theClass) {
	var allHTMLTags = new Array();
	var a = new Array();
	var j = 0;
	var allHTMLTags=document.getElementsByTagName("*");
	for (i=0; i<allHTMLTags.length; i++) {
		if (allHTMLTags[i].className==theClass) {
			j++;
			a[j] = allHTMLTags[i]; 
		}
	}
	return a;
}



function changeFontSize(inc,sizein) {
	var p = document.getElementById('souhrneksouhrn');
	var pt = document.getElementById('telickotelo');
	var p2 = document.getElementById('main_news');
	var p4 = document.getElementById('news_main_article');
	var p5 = getElementsByClass('detail-odstavec');
	if (p) {
		if(p.style.fontSize) {
			var size = parseFloat(p.style.fontSize.replace("em", ""));
		} else {
			var size = (getCookie("fsize")?getCookie("fsize"):1.2);
		}
		p.style.fontSize = (sizein?sizein:size+inc) + 'em';
		pt.style.fontSize = (sizein?sizein:size+inc) + 'em';

	}
	if (p2) {
		if(p2.style.fontSize) {
			var size = parseFloat(p2.style.fontSize.replace("em", ""));
		} else {
			var size = (getCookie("fsize")?getCookie("fsize"):1.2);
		}
		p2.style.fontSize = (sizein?sizein:size+inc) + 'em';
		
	}

	if (p4) {
		if(p4.style.fontSize) {
			var size = parseFloat(p4.style.fontSize.replace("em", ""));
		} else {
			var size = (getCookie("fsize")?getCookie("fsize"):1.2);
		}
		p4.style.fontSize = (sizein?sizein:size+inc) + 'em';
		
	}
	if (p5.length) {
		for (var x in p5) {
			if(p5[x].style.fontSize) {
				var size = parseFloat(p5[x].style.fontSize.replace("em", ""));
			} else {
				var size = (getCookie("fsize")?getCookie("fsize"):1.2);
			}
			p5[x].style.fontSize = (sizein?sizein:size+inc) + 'em';
		}
		
	}

	if (inc) {
		setCookie("fsize", size+inc, 1);
	}
	if (sizein) {
		setCookie("fsize", sizein, 1);
	}
}

function getCookie(c_name) {
var i,x,y,ARRcookies=document.cookie.split(";");
for (i=0;i<ARRcookies.length;i++) {
x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
  x=x.replace(/^\s+|\s+$/g,"");
  if (x==c_name) {
    return unescape(y);
    }
  }
}

function setCookie(c_name,value,exdays) {
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
document.cookie=c_name + "=" + c_value + "; path=/";
}
