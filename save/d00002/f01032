    <h1>Dining
    </h1>
    
 

    <ul id="AjaxTabs" class="clearfix ui-tabs-nav" style="margin-bottom:0px; display:none;">
        <li class="ui-tabs-selected"><a href="#tabOverview"><span>Overview</span></a></li>
       <li class=""><a href="#tabReviewTA"><span>TripAdvisor Review</span></a></li>

    </ul>
	
    <script type="text/javascript">
        $('#AjaxTabs').bind('tabsselect', function(event, ui) {
            // Objects available in the function context:
//          ui.tab     // anchor element of the selected (clicked) tab
//          ui.panel   // element, that contains the selected/clicked tab contents
//			ui.index   // zero-based index of the selected (clicked) tab
			
            var $tabs = $('#AjaxTabs').tabs();
            
            var selected = $tabs.tabs('option', 'selected'); // => 0
            
            if(selected == 0) {
            	s.linkTrackVars='sprop41';
            	s.prop41='Singapore Dining - Restaurants, Local Dishes & Other Cuisines';
