        <script type="text/javascript">
            function beginCountDown(){
               var timeURL = "http://meetings.apnic.net/33/admin/countdown/server-time?random=" + Math.floor(Math.random()*99999999) + "&date=" + new Date().valueOf();
                $.ajax({
                    url: timeURL,
                    context: document.body,
                    success: function(data){
                       var patt=/^\d+-\d+-\d+T\d+:\d+/gi; //yyyy-mm-ddThh:mm
                       if(patt.test(data)){
                           // Server time format looking good
                           var dateTime = data.split("T");
                           if(dateTime.length == 2){
                               var justDate = dateTime[0].split('-');
                               var justTime = dateTime[1].split(':');
                               if((justDate.length == 3)&&(justTime.length == 2)){
                                   // Server time broken up well for processing
                                   if(justDate[1].substring(0,1)=="0"){
                                       // MM (01) = M (1)
                                       justDate[1] = justDate[1].substring(1);
                                   }
                                   if(justDate[2].substring(0,1)=="0"){
                                       // DD (01) = D (1)
                                       justDate[2] = justDate[2].substring(1);
                                   }
                                   if(justTime[0].substring(0,1)=="0"){
                                       // HH (01) = HH (1)
                                       justTime[0] = justTime[0].substring(1);
                                   }
                                   if(justTime[1].length > 2){
                                       // MM (01\n) = MM (01)
                                       justTime[1] = justTime[1].substring(0,2);
                                   }
                                   if(justTime[1].substring(0,1)=="0"){
                                       // MM (01) = MM (1)
                                       justTime[1] = justTime[1].substring(1);
                                   }
                                   justDate[1]--;
                                   serverTime = new Date(justDate[0],justDate[1]--,justDate[2],justTime[0],justTime[1]);
                                   var dates = new Array();
                                   dates.push(new Array(new Date('2012','0','6','9','0'),"Nominations open"));
                                   dates.push(new Array(new Date('2012','1','10','17','30'),"Nominations close"));
                                   dates.push(new Array(new Date('2012','1','15','13','30'),"Proxy & Online voting opens"));
                                   dates.push(new Array(new Date('2012','1','29','13','30'),"Proxy & Online voting closes"));
                                   dates.push(new Array(new Date('2012','2','2','13','30'),"On-site voting opens"));
                                   dates.push(new Array(new Date('2012','2','2','18','30'),"On-site voting closes"));
                                   var hit = false;
                                   for (var i=0; i<dates.length; i++){
                                       var difference = dates[i][0] - serverTime;
                                       diff_days = Math.floor(difference/1000/60/60/24);
                                       diff_hours= Math.floor(((difference/1000/60/60/24) % 1) * 24);
                                       diff_mins = Math.floor(((((difference/1000/60/60/24) % 1) * 24) %1) * 60);
                                       if(difference > 0){
                                           hit = true;
                                           $('#time').html(dates[i][1] + " in " + diff_days + " days, " + diff_hours + " hours, " + diff_mins + " mins ");
                                           break;
                                           // 16200000 miliseconds between 4:30 and 10:00
                                       }
                                   }
                                   if(!hit){
                                       $('#time').html("");
                                   }
                               }
                           }
                       }
                    }
                });
            }

            $(document).ready(function(){
                beginCountDown();
                var interval = setInterval(beginCountDown,"20000");
            });
        </script>                    <div id="footer">
