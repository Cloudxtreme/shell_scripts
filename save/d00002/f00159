	function jsWebstats() {
		// netmonitor
		pp_gemius_hit(js_gemius_identifier);
		// GA
		_gaq.push(['_trackPageview']);
		// NV
		if ($) {
			var nvcSrc = $('#nvc img').remove().attr('src');
			nvcSrc = nvcSrc.replace(/;r=[^;]*/, ';r=' + Math.random());
			$('#nvc').html('<im' + 'g sr' + 'c="' + nvcSrc + '" alt="" />');
		}
	}
