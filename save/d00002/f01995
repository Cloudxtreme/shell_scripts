<script type="text/javascript">

function addTwitterWidget() {
  !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);
  }}(document,"script","twitter-wjs");
} 

function addLinkedIn() {
  $.getScript(document.location.protocol + '//platform.linkedin.com/in.js').done(function(script, textStatus) { /*IN.Parse();*/ });
} 

function addFacebook() {
  $.getScript(document.location.protocol + '//connect.facebook.net/nl_NL/all.js').done(function(script, textStatus) { FB.XFBML.parse(); });
} 

function addAddThis() {
  $('.addthis_toolbox').show();
  $.getScript(document.location.protocol + '//s7.addthis.com/js/250/addthis_widget.js#pubid=xa-4f98071e720d092a');
} 

// RF: These functions were in the cookiedetect script, but are dependant on scripts above. 
// So placed here to be initialised when cookies are allowed.
function activateSocialMediaElements() {
  //reload FB and linkedin
  $('.tweet-container').show();
  $('.socialmedia').show();
  $('.socialcookie').html('');
}

function activateAllCookieElements() {
  addTwitterWidget(); 
  addLinkedIn(); 
  addFacebook();
  addAddThis();
  activateSocialMediaElements();
  addAnalyticsScript();
  addSitestatJs();
  initSitestat();
}

$(document).ready(function(){
  // Initialize 
  $('.addthis_toolbox').hide();
  $('.tweet-container').hide();
  
  $('body').cookieDetect({
      boxHeader:'Cookies',
      boxText:'TNO is wettelijk verplicht cookies op haar website standaard uit te zetten. Cookies zijn echter handig om uw gebruiksgemak op tno.nl te verbeteren, bijvoorbeeld:<ul><li>door content makkelijk te kunnen delen op social media platforms als Facebook, Twitter en LinkedIn;</li><li>door aan de hand van statistieken de website beter in te richten.</li></ul>U kunt hieronder aangeven of u het gebruik van cookies wilt accepteren.',
      readmoreLink:'Meer informatie',
      readmoreUrl:'/content.cfm?context=overtno&content=overtnosub&laag1=282&item_id=163',
      acceptBtn:'&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Accepteren&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;',
      acceptDescr:'Klik hier om cookies te accepteren',
      msgclose:'Sluit en weiger cookies',
      msgtoggle:'Belangrijke informatie over cookie wetgeving'
	  
  });
  
  $('body').cookieSocial({
      targetDiv:'.socialmedia',
      boxHeader:'',
      boxText:'Deze social media functionaliteiten maken gebruik van cookies. Via de onderstaande knop kunt u aangeven of u het gebruik van cookies accepteert. Mocht u meer informatie willen, dan kunt u dit opvragen via de knop "meer informatie".',
      boxCss:'.socialmedia-hover{position:absolute;top:-172px;margin-left:70px;z-index:10;}',
      readmoreLink:'Meer informatie',
      readmoreUrl:'/content.cfm?context=overtno&content=overtnosub&laag1=282&item_id=163',
      acceptBtn:'&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Accepteren&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;',
      acceptDescr:'Belangrijke informatie over cookie wetgeving',
  });
  
  if(getCookie('hideCookiePopup') == 'true' ) { 
    activateAllCookieElements();
  } 
  
       
  /*var $reportCookieUrl = 'http://cookielog.flink.nl.remy.flink.dev';     
  //$.getJSON($reportCookieUrl+'?callback=?', function(data) { 
  //  console.log(data); 
  //});*/
  
});

