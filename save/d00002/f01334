// for google analytics crossdomain analize
$(document).ready(function(){
    var hosts = "f.msgs.jp";
    $('a').filter(function() {
        return this.hostname && this.hostname !== location.hostname && hosts.indexOf(this.hostname)!=-1;
    }).click(function(){
        var $elm = $(this);
        var target = $elm.attr("target");
        if(target === "_blank"){
            window.open(_gat._getTrackers()[0]._getLinkerUrl($elm.attr("href")));
        }else{
            _gaq.push(['_link', $elm.attr("href")]);
        }    
    
        return false;
    });
});
</script>
