			if (body.length > 0) {
				body[0].className += ' adp-square-banner';
			}
		}
	});
	</script>
	<!--  end slave  -->

</div>
<div id="ad-supersquare-video" class="ad">
	<h4 class="admarker">reklama</h4>

	
	<!-- start slave -->
	<div id="adoceanczvarrqrlthx"></div>
	<script type="text/javascript">
	/* (c)AdOcean 2003-2009, iinfo_cz.Lupa.cz.Lupa.cz.Square banner - video */
	ado.slave('adoceanczvarrqrlthx', { myMaster: 'Wc.67LGCQzlIhJg7wDIrLaJk0juvCD6ZF._tm7tvEvz.o7', onServerEmission: function() {
